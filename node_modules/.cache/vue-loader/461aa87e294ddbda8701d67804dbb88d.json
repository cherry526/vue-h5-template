oss.wbgj.cn/vip_legal_right/15943601460001594360145503_595808.jpeg',\r\n        ],\r\n        remainTime: 3999000,\r\n        selConsumer: {},\r\n        channelType: '',\r\n        btnDisabled: false,\r\n        options: this.$route.query || {},\r\n        suitShops : [\r\n          {\r\n            address: \"南山区高新园\",\r\n            brandName: null,\r\n            cancelOfPayment: true,\r\n            distance: 5349.90904675849,\r\n            existsAct: true,\r\n            existsBaseDiscount: false,\r\n            lat: 22.53332,\r\n            lng: 113.93041,\r\n            shopId: 47,\r\n            shopName: \"余李冰的店3\",\r\n            shopPic: \"https://toss.wbgj.cn/jkb_shop/1550298855000_767071.png\",\r\n            shopSn: \"shop_7l44IXU2qRd\",\r\n            tel: \"15218130026\"\r\n          }\r\n        ],\r\n        info: {\r\n          shopShow: true,\r\n          lables:'1, 2, 3',\r\n          ableUseWallet: true,\r\n          actBannerList: [],\r\n          actEndTime: 1682783999000,\r\n          actExplain: null,\r\n          actId: 4094,\r\n          actName: \"余-特卖-渠道\",\r\n          shopName: '哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈',\r\n          shopPic: \"https://toss.wbgj.cn/vip_legal_right/15874517480001587451747311_826202.jpeg\",\r\n          actRecommendReason: \"\",\r\n          actRule: null,\r\n          actStartTime: 1587398400000,\r\n          actState: 2,\r\n          actType: \"ACT_SPECIAL_OFFER\",\r\n          backgroundPic: null,\r\n          backgroundType: 0,\r\n          buyLimit: 1,\r\n          childEnumKey: \"ACT_SPECIAL_OFFER\",\r\n          countDownHour: 0,\r\n          limitActState: null,\r\n          limitCount: null,\r\n          limitEndTime: null,\r\n          limitFrequency: 0,\r\n          limitStartTime: null,\r\n          limitState: false,\r\n          mediaList: [],\r\n          noVipPrice: 0,\r\n          offBackBuy: 0,\r\n          originalPrice: 10000,\r\n          panicBuyingPrice: 0,\r\n          promotion: null,\r\n          r