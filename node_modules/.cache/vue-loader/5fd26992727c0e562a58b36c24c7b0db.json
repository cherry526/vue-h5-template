{"remainingRequest":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\src\\views\\order\\refund.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\src\\views\\order\\refund.vue","mtime":1596624611566},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["refund.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8IA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"refund.vue","sourceRoot":"src/views/order","sourcesContent":["<!--\r\nimport { setTimeout } from 'timers';\r\n * @Description: \r\n * @Version: 1.0\r\n * @Autor: yesong\r\n * @Date: 2020-08-04 09:48:35\r\n * @LastEditors: yesong\r\n * @LastEditTime: 2020-08-04 19:11:56\r\n--> \r\n<template>\r\n  <div>\r\n    <!-- 申请退款页面 -->\r\n    <div v-if=\"!submit\">\r\n      <div class=\"act flex-v\">\r\n        <img :src=\"info.actInfo.actPic\"/>\r\n        <div class=\"flex-1 ft30 c1A act-header\">\r\n          <!-- 礼包显示活动名称 -->\r\n          <template v-if=\"info.actInfo.actType == 'ACT_GIFT_PACKAGE' || info.actInfo.actType == 'ACT_CASH_VOUCHER'\">\r\n            <div class=\"bold name text-two\">{{ info.actInfo.actName || '活动名称' }}</div>\r\n          </template>\r\n          <!-- 特卖显示店铺名称+卡券名称 -->\r\n          <template v-else-if=\"info.actInfo.actType == 'ACT_SPECIAL_OFFER'\">\r\n            <div class=\"bold name t-elps\">{{ info.shopInfo.shopName || '店铺名称' }}</div>\r\n            <div class=\"bold name t-elps\">{{ info.cardInfo.cardName || '卡券名称' }}</div>\r\n          </template>\r\n          <div class=\"ft26 cCD lh1 rules\" v-if=\"info.orderInfo.orderType == 5\">\r\n            会员卡不可退款，仅可退购买卡券\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class='info-line flex-v' >\r\n        <span class='flex-1 ft32 c1A'>\r\n          <span class=\"bold\">退款数量</span>\r\n        </span>\r\n        <div class='flex-v'>\r\n          <span @click='reduceNum' :class=\"[num == 1 ? 'cC2' : '', 'option']\" id=\"STATIS_SUB\">-</span>\r\n          <input type=\"number\" v-model=\"num\" class='numInput tac bold ft32' @blur=\"blurInput\"/>\r\n          <span @click='addNum' class='option' id=\"STATIS_ADD\">+</span>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"block p-right-30\">\r\n        <div class=\"refund-price\">\r\n          <div class=\"flex-b\">\r\n            <span class=\"ft32 bold c1A\">退款金额</span>\r\n            <div class=\"ft20 cF51\">\r\n              ¥\r\n              <span class=\"ft36 cF51 bold\">{{ totalMoney/100}}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"ft26 c9E tar\">1-7个工作日内退款至原付款账户</div>\r\n      </div>\r\n      <!-- 选择退款原因 -->\r\n      <div class=\"refund_cause flex-c-b ft32\" @click=\"handleSelect\">\r\n        <div class=\"bold\">退款原因</div>\r\n        <div class=\"select flex-cc\">\r\n          <span :class=\"['span-one', causeIndex != -1 ? 'c1A' : '']\">{{causeIndex == -1? '请选择': causeList[causeIndex].name}}</span>\r\n          <span class=\"iconfont iconicon_more c9E ft18\"></span>\r\n        </div>\r\n      </div>\r\n      <div class=\"other_refund_cause\">\r\n        <div class=\"area\" v-if=\"causeIndex == 11\">\r\n          <textarea v-if=\"showtextarea\" bindblur=\"blur\" v-model=\"value\" maxlength=\"100\" auto-focus=\"true\" placeholder-style=\"color: #C2C3C5;\" placeholder=\"更多不爽？必须吐槽！倾听你任何的不爽…\"></textarea>\r\n          <div v-else class='textarea scroll-y' scroll-y=\"true\" bindtap=\"focus\">\r\n            <div v-if=\"value\">{{ value }}</div>\r\n            <div v-else style='color: #C2C3C5;'>更多不爽？必须吐槽！倾听你任何的不爽…</div>\r\n          </div>\r\n          <div class=\"num ft30 c3C\">{{ value.length }}/100</div>\r\n        </div>\r\n      </div>\r\n      <!-- 上传凭证 -->\r\n      <div class=\"upload\">\r\n        <div class=\"flex-aend bor w\">\r\n          <div class=\"ft32 c1A bold\">上传凭证</div>\r\n          <div class=\"ft24 c9E\" style=\"margin-left: 6px;\">\r\n            /最多可上传\r\n            <span class=\"cF51\">5</span>\r\n            张\r\n          </div>\r\n        </div>\r\n        <div class=\"list flex-wrap\">\r\n          <upload-img\r\n              :imgs.sync=\"imgList\"\r\n              :uploadImgNum=\"5\"\r\n            >\r\n          <div slot=\"uploadImg\" class=\"item one tac flex-v\">\r\n            <div class=\"flex-1\">\r\n              <img src=\"@/assets/imgs/camera_icon.png\"/>\r\n              <div>上传凭证</div>\r\n            </div>\r\n          </div>\r\n          </upload-img>\r\n          <template v-if=\"imgList\">\r\n            <div class=\"item\" v-for=\"(item, index) in imgList\" :key=\"index\">\r\n              <img :src=\"item\"/>\r\n              <img class=\"close\" src=\"@/assets/imgs/close_fb.png\" @click=\"deleteImg(index)\"/>\r\n            </div>\r\n          </template>\r\n        </div>\r\n      </div>\r\n      <!-- 底部按钮 -->\r\n      <div class=\"btn bold\" @click=\"cancle\">\r\n        <button>提交</button>\r\n      </div>\r\n    </div>\r\n    <!-- 提交成功 -->\r\n    <div class=\"success\" v-else>\r\n      <div class=\" flex-co-c w success-box\">\r\n        <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/icon_success.png\"/>\r\n        <div class=\"ft40 c1A bold tijiao\">提交成功</div>\r\n        <span class=\"ft28 tips\">\r\n          您的退款申请已提交成功，\r\n          <span class=\"cD1\">1-7个工作日</span>\r\n          内退款至原付款账户\r\n        </span>\r\n        <div class=\"nav flex-cc\" hover-class=\"none\" @click=\"$router.back(-1)\">返回订单</div>\r\n      </div>\r\n    </div>\r\n    <!-- 退款原因选择弹窗 -->\r\n    <div class=\"mask\" v-if=\"showCausePop\">\r\n      <div class=\"cause_popup\">\r\n        <div class=\"ft36 flex-c title\">退款原因</div>\r\n        <span class=\"iconfont iconicon_close ft24\" @click=\"handleClose\"></span>\r\n        <div class=\"scroll scroll-y\" scroll-y>\r\n          <div v-for=\"(item, index) in causeList\" :key=\"index\">\r\n            <div class=\"cause_item flex-c-b ft30\" @click=\"handleSelectCause(index)\">\r\n              <span class=\"\">{{item.name}}</span>\r\n              <div class=\"checked\" v-if=\"causeIndex != index\"></div>\r\n              <img class=\"\" src=\"@/assets/imgs/cause_checked.png\" v-else/>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"bottom_btn flex-cc\">\r\n          <button class=\"default\" size=\"default\" type=\"default\" @click=\"handleClose\">确认选择</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      causeList: [\r\n        {name: '计划有变，没时间消费'}, \r\n        {name: '商家说可以直接以小程序上的优惠价格线下消费'}, \r\n        {name: '商家停业/装修/转让'},\r\n        {name: '去过了，不太满意'},\r\n        {name: '朋友/网上评价不太好'},\r\n        {name: '预约不上'},\r\n        {name: '有更优惠的活动，退款后重新再买'},\r\n        {name: '商家营业但不接待'},\r\n        {name: '后悔了，不想要了'},\r\n        {name: '买多了/买错了'},\r\n        {name: '其他原因'},\r\n      ],\r\n      submit: false,\r\n      causeIndex: -1,\r\n      value: '',\r\n      showTextarea: true,\r\n      num:1,\r\n      showCausePop: false,\r\n      totalMoney:0,\r\n      info: {},\r\n      imgList: []\r\n    }\r\n  },\r\n  watch: {\r\n    num(newVal, oldVal) {\r\n      this.handlerNum(newVal)\r\n    }\r\n  },\r\n  methods: {\r\n    // 退款原因选择框\r\n    handleSelect(){\r\n      this.showCausePop = true\r\n    },\r\n    // 关闭选择框\r\n    handleClose(){\r\n      this.showCausePop = false\r\n    },\r\n    // 单选 退款原因\r\n    handleSelectCause(index) {\r\n      this.causeIndex = index\r\n    },\r\n\r\n    // 退款数量减\r\n    reduceNum() {\r\n      if (this.num > 1) {\r\n        this.num = this.num - 1\r\n        this.totalMoney = Math.floor(((this.num-1)*this.info.cardInfo.actBuyMoney) / this.info.cardInfo.actBuyCnt)\r\n      }\r\n    },\r\n    \r\n    // 输入框失去焦点，如果当前输入框的值为空，把数量设为1\r\n    blurInput() {\r\n      if(!this.num) {\r\n        this.num = 1\r\n      }\r\n      this.totalMoney = Math.floor((this.num*this.info.cardInfo.actBuyMoney) / this.info.cardInfo.actBuyCnt)\r\n    },\r\n\r\n    addNum() {\r\n      console.log(this.info.cardInfo,1122)\r\n      if (this.num+1 <= this.info.cardInfo.ableRefundNum) {\r\n        this.num = this.num + 1\r\n        this.totalMoney = Math.floor(((this.num+1)*this.info.cardInfo.actBuyMoney) / this.info.cardInfo.actBuyCnt)\r\n      } else {\r\n        this.$Toast(`退款数量最大限度为${this.info.cardInfo.ableRefundNum}`)\r\n      }\r\n    },\r\n\r\n    handlerNum(num) {\r\n      if (num > this.info.cardInfo.ableRefundNum) {\r\n        this.num = this.info.cardInfo.ableRefundNum\r\n        this.$Toast(`退款数量最大限度为${this.info.cardInfo.ableRefundNum}`)\r\n      }\r\n    },\r\n\r\n    focus () {\r\n      this.showTextarea = true\r\n    },\r\n\r\n    blur () {\r\n      this.showTextarea = false\r\n    },\r\n\r\n    // 删除图片\r\n    deleteImg(index) {\r\n      this.imgList.splice(index, 1)\r\n    },\r\n\r\n    // 申请退款\r\n    cancle () {\r\n      if(this.isCanClick) return\r\n      if( !this.verify() ) return // 校验表单\r\n      this.isCanClick = true\r\n      this.$Toast.loading({\r\n        message: '加载中...',\r\n        forbidClick: true,\r\n        duration: 0\r\n      });\r\n      this.$api.orderRefundApply({\r\n        refundReason: this.causeIndex == 11 ? this.value.replace(/(^\\s*)|(\\s*$)/g, \"\") : this.causeList[this.causeIndex].name,\r\n        refundEnum: Number(this.causeIndex)+1,\r\n        refundImages: this.imgList.join(','),\r\n        refundFee: this.totalMoney,\r\n        refundNum: this.num,\r\n        tradeId: this.tradeId\r\n      }).then(res => {\r\n        if (res.state) {\r\n          this.submit = true\r\n          // app.globalData.hasUpdateCenter = true // 刷新会员中心页\r\n        } else {\r\n          setTimeout(()=> {\r\n            this.$Toast(res.message)\r\n          }, 100)\r\n        }\r\n      }).finally(()=>{\r\n        this.$Toast.clear()\r\n        this.isCanClick = false\r\n      })\r\n    },\r\n\r\n    verify () { // 校验表单\r\n      let causeIndex = this.causeIndex\r\n      let value = this.value.replace(/(^\\s*)|(\\s*$)/g, \"\") // 去掉前后空格\r\n      if (causeIndex == -1) {\r\n        this.$Toast('请选择退款原因')\r\n        return false\r\n      } else if (causeIndex == 11 && value == '') {\r\n        this.$Toast('请输入退款原因，限100字')\r\n        return false\r\n      } else {\r\n        return true\r\n      }\r\n    },\r\n\r\n    //预览图片\r\n    previewImage(e) {\r\n      let index = e.currentTarget.dataset.index\r\n      wx.previewImage({\r\n        current: index, // 当前显示图片的http链接\r\n        urls: this.imgList // 需要预览的图片http链接列表\r\n      })\r\n    },\r\n  },\r\n  created() {\r\n    let options = this.$route.query\r\n    let info = this.$save.get('session', 'refund') || ''\r\n    this.info = info\r\n    this.tradeId = options.tradeId || ''\r\n    this.num = info.cardInfo.ableRefundNum\r\n    this.totalMoney = info.cardInfo.ableRefundMoney\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.act {\r\n  padding: 35px 30px;\r\n  background: #fff;\r\n  .act-header {\r\n    height: 160px;\r\n  }\r\n}\r\n.scroll-y {\r\n  overflow-y: auto\r\n}\r\n.flex-aend {\r\n  display: flex;\r\n  align-items: flex-end;\r\n}\r\n\r\n\r\n.act img {\r\n  width: 160px;\r\n  height: 160px;\r\n  margin-right: 24px;\r\n  border-radius: 12px;\r\n}\r\n.act .name {\r\n  line-height: 42px;\r\n}\r\n.act .rules {\r\n  margin-top: 18px;\r\n}\r\n.block {\r\n  padding: 40px 0px 40px 30px;\r\n  background-color: #fff;\r\n}\r\n.refund-price {\r\n  margin-bottom: 18px;\r\n}\r\n.cCD {\r\n  color: #CD7B00;\r\n}\r\n\r\n.select img, .select .img{\r\n  width: 38px;\r\n  height: 38px;\r\n}\r\n.select .img {\r\n  border-radius: 4px;\r\n  border: 2px solid #E0E1E4;\r\n  box-sizing: border-box;\r\n}\r\n.select .item {\r\n  padding: 27px 24px 27px 0;\r\n  border-bottom: 1px solid #F6F6F6;\r\n}\r\n.select .item:last-child {\r\n  border: none;\r\n}\r\n.bor {\r\n  border-bottom: 1px solid #F6F6F6;\r\n  padding-bottom: 24px;\r\n}\r\n\r\n.upload {\r\n  margin-top: 20px;\r\n  padding: 30px  0 30px 24px;\r\n  background: #fff;\r\n  flex-wrap: wrap;\r\n}\r\n.upload .list {\r\n  padding-right: 24px;\r\n}\r\n.upload .list .one {\r\n  width: 140px;\r\n  height: 140px;\r\n  background: #F4F5F7;\r\n  border: 1px solid #F6F6F6;\r\n  border-radius: 4px;\r\n}\r\n.upload .list .one img {\r\n  width: 40px;\r\n  height: 40px;\r\n  margin-bottom: 20px;\r\n  display: inline-block;\r\n}\r\n.upload .list .one div {\r\n  font-size: 24px;\r\n  color:#A9ABB3;\r\n}\r\n.upload .list .item {\r\n  position: relative;\r\n  