r\n * @LastEditors: cherry\r\n * @LastEditTime: 2020-07-21 16:32:56\r\n--> \r\n<template>\r\n  <div>\r\n    <div class='shop-item flex' v-for='(item, index) in list' :key='index' @click='_toShopDetail(item.shopSn)'>\r\n      <div class='shop-pic flex-v'>\r\n        <img :src='item.shopPic'></img>\r\n      </div>\r\n      <div class='flex-1'>\r\n        <div class='shop-name t-elps ft34 c1A bold'>{{item.shopName}}</div>\r\n        <div class='flex-v ft24 l1 cCD shop-dis'>\r\n          <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/icon_hot.png\" class='icon_home_list_hot'></img>\r\n          <span class=\"ft24\">人气{{item.shopVisitCnt}}</span>\r\n          <span class='c9E  flex-1 flex-end' v-if=\"item.distance\">{{item.distance}}</span>\r\n        </div>\r\n        <div class='flex-v ft24 c66 l33'>\r\n          <span class='mr30 ft24' v-if=\"item.busCircleName || item.ctgName\">{{item.busCircleName || ''}}<span v-if=\"item.busCircleName && item.ctgName\"> | </span>{{item.ctgName || ''}}</span>\r\n          <span>人均{{item.perCapita}}</span>\r\n        </div>\r\n        <div class='flex-v flex-wrap label-line' v-if=\"item.shopLabels\">\r\n          <span class='shop-label ft20 c5D' v-for=\"(item, i) in item.shopLabels\" :key=\"i\">{{i}}</span>\r\n        </div>\r\n        <div class='flex-v fx-line' v-if=\"item.vipCashbackMinRate || item.vipCashbackMaxRate\">\r\n          <span class='fx-label vip-fan-label'>返</span>\r\n          <span class='ft24 c66'>会员日返现{{item.vipCashbackMinRate/100}}%-{{item.vipCashbackMaxRate/100}}%</span>\r\n        </div>\r\n        <div class='flex-v fx-line' v-if=\"item.pacificCashbackMinRate || item.pacificCashbackMaxRate\">\r\n          <span class='fx-label normal-fan-label cff'>返</span>\r\n          <span class='ft24 c66'>平日返现{{item.pacificCashbackMinRate/100}}%-{{item.pacificCashbackMaxRate/100}}%</span>\r\n        </div>\r\n        <div class='flex-v fx-line' v-if=\"item.q_str\">\r\n          <span class='fx-label quan-label cff'>券</span>\r\n          <div class='ft24 c66 t-elps'>{{item.q_str}}</div>\r\n        </div>\r\n        <div class='flex-v fx-line' v-if=\"item.t_str\">\r\n           <span class='fx-label tuan-label cff'>特</span>\r\n          <div class='ft24 c66 t-elps'>{{item.t_str}}</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    name: 'shopReturnItem',\r\n    components: {},\r\n    props: {\r\n      // list: {\r\n      //   type: Array,\r\n      //   value: []\r\n      // },\r\n    },\r\n    data() {\r\n      return {\r\n        list: [\r\n          {\r\n            \"shopName\": \"老上海馄饨\",\r\n            \"shopLat\": 1,\r\n            \"shopLng\": 1,\r\n            \"distance\": 23232323,\r\n            \"perCapita\": 15,\r\n            \"shopPic\": \"https://wx.qlogo.cn/mmhead/Q3auHgzwzM7tliaTsRf8cvbKLSQbreyibJIRq1pzjrsuf59vM7o6Pa3A/0\",\r\n            \"shopSn\": \"\",\r\n            \"shopLabels\": \"馄饨,饺子,面食\",\r\n            \"busCircleName\": \"西乡\",\r\n            \"ctgName\": \"面食\",\r\n            \"vipCashbackMinRate\": 1,\r\n            \"vipCashbackMaxRate\": 1,\r\n            \"pacificCashbackMinRate\": 1,\r\n            \"pacificCashbackMaxRate\": 1,\r\n            \"cardList\": [\r\n              {\r\n                \"cardName\": \"\",\r\n                \"cardType\": \"\",\r\n                \"originalprice\": 1,\r\n                \"sellPrice\": 1,\r\n                \"useCondition\": \"\"\r\n              }\r\n            ],\r\n            \"shopVisitCnt\": 1,\r\n            \"q_str\": 0,\r\n            \"t_str\": 1\r\n          }\r\n        ]\r\n      }\r\n    },\r\n    methods: {\r\n    },\r\n    created() {\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n  .shop-item{\r\n    padding-bottom: 70px;\r\n  }\r\n  .shop-pic{\r\n    max-width: 160px;\r\n    min-width: 160px;\r\n    height: 160px;\r\n    border-radius: 6px;\r\n    border: 1px solid #E5E6E8;\r\n    overflow: hidden;\r\n    margin-right: 18px;\r\n    box-sizing: border-box;\r\n  }\r\n  .shop-pic image{\r\n    width: 100%;\r\n    height: 100%;\r\n    border-radius: 6px;\r\n  }\r\n  .shop-name{\r\n    max-width: 500px;\r\n  }\r\n  .shop-label{\r\n    background: #FEEED5;\r\n    border-radius: 3px;\r\n    padding: 6px 12px;\r\n    margin-top: 20px;\r\n    margin-right: 10px;\r\n    color: #CD7B00;\r\n    line-height: 1;\r\n  }\r\n  .shop-dis{\r\n    margin: 18px 0 20px;\r\n  }\r\n  .vip-label{\r\n    background: #FFFFFF;\r\n    border: 1px solid #E5E6E8;\r\n    border-radius: 3px;\r\n    padding: 3px 8px;\r\n    margin-right: 20px;\r\n  }\r\n  .label-line{\r\n    max-height: 50px;\r\n    overflow: hidden;\r\n  }\r\n  .icon_home_list_hot{\r\n    width: 24px;\r\n    height: 24px;\r\n    margin-right: 5px;\r\n  }\r\n  .fx-label{\r\n    display: inline-block;\r\n    text-align: center;\r\n    min-width: 28px;\r\n    max-width: 28px;\r\n    line-height: 28px;\r\n    margin-right: 8px;\r\n    border-radius: 3px;\r\n    font-size: 20px;\r\n  }\r\n  .vip-fan-label{\r\n    background-image: linear-gradient(90deg, #401E07 0%, #201006 100%);\r\n    color: #FFD494;\r\n  }\r\n  .normal-fan-label{\r\n    background-image: linear-gradient(270deg, #EBBB86 0%, #E9AF85 100%);\r\n  }\r\n  .quan-label{\r\n    background-image: linear-gradient(90deg, #FF7120 0%, #FF411D 100%);\r\n  }\r\n  .tuan-label{\r\n    background-image: linear-gradient(135deg, #FFD422 0%, #FF8208 100%);\r\n  }\r\n  .fx-line{\r\n    margin-top: 18px;\r\n  }\r\n\r\n  .fx-line .t-elps, .tuan-label .t-elps {\r\n    max-width: 500px;\r\n  }\r\n</style>"]}]}