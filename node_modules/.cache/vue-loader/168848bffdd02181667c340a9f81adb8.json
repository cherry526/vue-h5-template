CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"activeFooter.vue","sourceRoot":"src/components/active","sourcesContent":["<!--\r\n * @Description: 活动底部\r\n * @Version: 1.0\r\n * @Autor: cherry\r\n * @Date: 2020-07-22 18:01:22\r\n * @LastEditors: cherry\r\n * @LastEditTime: 2020-07-31 11:17:37\r\n--> \r\n<template>\r\n  <div>\r\n    <!-- 虚拟产品footer -->\r\n    <template v-if=\"!info.iosShowPrice && isIos && info.actType == 'ACT_SPECIAL_OFFER'\"> \r\n      <div class=\"flex-cc footer_no\"> iOS手机暂不支持购买 </div>\r\n    </template>\r\n    <template v-else>\r\n      <!-- 限时限量底部 -->\r\n      <template v-if=\"info.childEnumKey == 'ACT_SPECIAL_OFFER_PURCHASE_LIMIT' || info.childEnumKey == 'ACT_GIFT_PACKAGE_PURCHASE_LIMIT'\">\r\n        <div class=\"skill_footer\" v-if=\"info.limitActState != 12\">\r\n          <div class=\"footer_tips flex-cc\" v-if=\"info.timeState == 3\"> {{activeType ? '限量秒杀' : '限时抢购'}}活动已结束～</div>\r\n          <div class=\"footer_btn w flex-c-b\" :class=\"info.timeState == 1 ? 'footer_yellow': (btnText == '立即抢购') ? 'footer_red' : info.timeState == 3 ? 'footer_gray' : 'footer_gray' \">\r\n            <!-- 消费券渠道进入显示价格 -->\r\n            <template v-if=\" info.channelType == 'CHANNEL_CONSUMER_COUPON' || info.channelType == 'CHANNEL_FORTUNE_COUPON'\">\r\n              <div class=\"flex\">\r\n                <template v-if=\" selConsumer.couponMoney\">\r\n                  <div class=\"cFFD ft24\" >\r\n                    券后￥<span class=\"ft50 bold\">{{ (info.worth - selConsumer.couponMoney) > 0 ? (((info.worth - selConsumer.couponMoney)/100) | formatFloat) : 0 }}</span>\r\n                  </div>\r\n                  <span class=\"ft26 cff ml20\" style=\"opacity: 0.7;\">原价¥{{ info.worth/100 }}</span>\r\n                </template>\r\n                <template v-else>\r\n                  <div class=\"cFFD ft24\">\r\n                    ￥<span class=\"ft50 bold\">{{ info.worth/100 }}</span>\r\n                  </div>\r\n                </template>\r\n              </div>\r\n            </template>\r\n            <div v-else class=\"footer_left ft26 flex-b-e\">\r\n              <div class=\"footer_buyrule mg4\">抢购价</div>\r\n              <div class=\"flex-b-e color_y\">\r\n                <span class=\"ft28\">¥</span>\r\n                <span class=\"bold ft50 lh1\" v-if=\"info.buyLimit == 2\">{{info.sellPrice/100 || 0}}</span>\r\n                <span class=\"bold ft50 lh1\" v-if=\"info.buyLimit == 1\">{{info.noVipPrice/100 || 0}}</span>\r\n              </div>\r\n              <div class=\"footer_buyrule\" v-if=\"info.buyLimit == 2\">仅限会员</div>\r\n              <div class=\"footer_buyrule\" v-else>非会员价¥{{ info.noVipPrice/100 || 0 }}</div>\r\n            </div>\r\n            <!-- 活动已发布 -->\r\n            <div class=\"footer_right flex-v bold\"  @click=\"remindSet\" v-if=\"info.timeState == 1\">\r\n              <div id=\"skillSubscribe\" :class=\"btnText.length == 3 ? 'mr18' : ''\"> {{btnText}} </div>\r\n            </div>\r\n            <template v-else>\r\n              <div class=\"flex-b-e\" @click=\"handlerEvent\">\r\n                <div class=\"tac submit-btn-linmited\">\r\n                  <button class=\"footer_right flex-v bold\" :class=\" btnText.length == 3 ? 'mr18' : ''\" id=\"seckillBuy\">{{btnText}}</button>\r\n                  <template v-if=\" !btnDisabled && selCashred.money && (info.sellPrice || info.noVipPrice) && info.channelType != 'CHANNEL_CONSUMER_COUPON' && info.channelType != 'CHANNEL_FORTUNE_COUPON'\">\r\n                    <!-- 显示会员券后价 -->\r\n                    <div  class=\"ft26 c99\" v-if=\"(info.buyLimit == 1 && vipUser) || info.buyLimit == 2\">券后价¥{{ (info.sellPrice - selCashred.money) > 0 ?  (((info.sellPrice - selCashred.money)/100) | formatFloat) : 0 }}</div>\r\n                    <!-- 显示非会员券后价 -->\r\n                    <div  class=\"ft26 c99\" v-else-if=\"info.buyLimit == 1\">券后价¥{{ (info.noVipPrice - selCashred.money) > 0 ? (((info.noVipPrice - selCashred.money)/100) | formatFloat) : 0 }}</div>\r\n                  </template>\r\n                </div>\r\n              </div>\r\n            </template>\r\n          </div>\r\n        </div>\r\n        \r\n        <div class=\"skill_footer\" v-else>\r\n          <div class=\"footer_btn w flex-c-b footer_gray\">\r\n            <!-- 消费券渠道进入显示价格 -->\r\n            <template v-if=\" info.channelType == 'CHANNEL_CONSUMER_COUPON' || info.channelType == 'CHANNEL_FORTUNE_COUPON'\">\r\n              <div class=\"flex\">\r\n                <template v-if=\" selConsumer.couponMoney\">\r\n                  <div class=\"cFFD ft24\" v-if=\" selConsumer.couponMoney\">\r\n                    券后￥<span class=\"ft50 bold\">{{ (info.worth - selConsumer.couponMoney) > 0 ? (((info.worth - selConsumer.couponMoney)/100) | formatFloat) : 0 }}</span>\r\n                  </div>\r\n                  <span class=\"ft26 cff ml20\" style=\"opacity: 0.7;\">\r\n                    原价¥{{ info.worth/100 }}\r\n                  </span>\r\n                </template>\r\n                <template v-else>\r\n                  <div class=\"cFFD ft24\">\r\n                    ￥<span class=\"ft50 bold\">{{ info.worth/100 }}</span>\r\n                  </div>\r\n                </template>\r\n              </div>\r\n            </template>\r\n            <div v-else class=\"footer_left ft26 flex-b-e\">\r\n              <div class=\"flex-b-e color_y\">\r\n                <span class=\"ft28\">¥</span>\r\n                <span class=\"bold ft50 lh1\" v-if=\"info.buyLimit == 2\">{{info.sellPrice/100 || 0}}</span>\r\n                <span class=\"bold ft50 lh1\" v-if=\"info.buyLimit == 1\">{{info.noVipPrice/100 || 0}}</span>\r\n              </div>\r\n              <div class=\"footer_buyrule\" v-if=\"info.buyLimit == 2\">仅限会员</div>\r\n              <div class=\"footer_buyrule\" v-else>非会员价¥{{ info.noVipPrice/100 || 0 }}</div>\r\n            </div>\r\n            <div class=\"footer_right flex-v bold\" >\r\n              <span class=\"mr18\">已抢完</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </template>\r\n      <!-- 普通活动特卖/大礼包 -->\r\n      <div class=\"active-footer\" v-else>\r\n        <!-- 普通特卖按钮 -->\r\n        <img v-if=\"!btnDisabled\" src='http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/button_05.png'>\r\n        <img v-else src='http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/button_03.png'>\r\n        <div class='flex-v poa footer-text'>\r\n          <!-- 消费券渠道进入显示价格 -->\r\n          <template v-if=\" info.channelType == 'CHANNEL_CONSUMER_COUPON' || info.channelType == 'CHANNEL_FORTUNE_COUPON' \">\r\n            <div class=\"flex flex-1\">\r\n              <template v-if=\" selConsumer.couponMoney\">\r\n                <div class=\"cFFD ft24\" >\r\n                  券后￥<span class=\"ft50 bold\">{{ (info.worth - selConsumer.couponMoney) > 0 ? (((info.worth - selConsumer.couponMoney)/100) | formatFloat) : 0 }}</span>\r\n                </div>\r\n                <span class=\"ft26 cff ml20\" style=\"opacity: 0.7;\">\r\n                  原价¥{{ info.worth/100 }}\r\n                </span>\r\n              </template>\r\n              <template v-else>\r\n                <div class=\"cFFD ft24\" >\r\n                  ￥<span class=\"ft50 bold\">{{ info.worth/100 }}</span>\r\n                </div>\r\n              </template>\r\n            </div>\r\n          </template>\r\n\r\n          <!-- 大礼包价格显示 -->\r\n          <template v-else-if=\"info.actType == 'ACT_GIFT_PACKAGE'\">\r\n            <!-- 售卖 -->\r\n            <div class='flex-1' v-if=\"info.channelTrack ? info.packageGiftType == 1 : info.restrictState == 2\">\r\n              <!-- 仅限会员购买 -->\r\n              <template v-if=\"info.buyLimit == 2\">\r\n                <span class='cFFD' v-if=\"info.sellPrice > 0\">¥<span class='ft50 bold'>{{info.sellPrice/100 || 0}}</span></span>\r\n                <span class='ft50 bold cFFD'  v-if=\"!info.sellPrice\">免费领</span>\r\n                <span class=\"limit-text\">仅限会员</span>\r\n              </template>\r\n              \r\n              <!-- 都可购买 -->\r\n              <template v-else>\r\n                <!-- 渠道跟踪 -->\r\n                <span class='ft50 bold cFFD'  v-if=\"info.channelTrack && !info.sellPrice && !info.noVipPrice\">免费领</span>\r\n                <!-- 专属二维码 -->\r\n                <span class='ft50 bold cFFD'  v-if=\"!info.channelTrack && !info.sellPrice && !info.noVipPrice\">限时免费</span>\r\n                <template v-else>\r\n                  <span class='cFFD'>¥<span class='ft50 bold'>{{info.sellPrice/100 || 0}}</span></span>\r\n                  <span class=\"limit-text\">非会员¥{{info.noVipPrice/100 || 0}}</span>\r\n                </template>\r\n              </template>\r\n            </div>\r\n        \r\n            <!-- 领取 -->\r\n            <div class='flex-1 flex-v' v-if=\"info.channelTrack ? info.packageGiftType == 2 : info.restrictState == 1\">\r\n              <span class='ft50 bold cFFD' v-if=\"info.freeReceiveType != 1\">免费领取</span>\r\n              <span class='ft50 bold cFFD' v-if=\"info.freeReceiveType == 1\">免费领取</span>\r\n              <span class=\"limit-text\" v-if=\"info.freeReceiveType == 2\">仅限新用户</span>\r\n              <span class=\"limit-text\" v-if=\"info.freeReceiveType == 3\">仅限会员</span>\r\n              <span class=\"limit-text\" v-if=\"info.freeReceiveType == 4\">仅限非会员</span>\r\n            </div>\r\n          </template>\r\n          <template v-else>\r\n            <!-- 普通特卖进入 -->\r\n            <div class='flex-1'>\r\n              <!-- 仅限会员购买 -->\r\n              <template v-if=\"info.buyLimit == 2\">\r\n                <!-- 会员免费领，仅限会员 -->\r\n                <template v-if=\"!info.sellPrice\">\r\n                  <span class='cFFD ft50 bold'>会员免费</span>\r\n                  <span class=\"limit-text\">仅限会员</span>\r\n                </template>\r\n                <template v-else>\r\n                  <span class='cFFD'>\r\n                    <span class=\"ft28\">¥</span>\r\n                    <!-- 自我游卡券显示价格区间 -->\r\n                    <template v-if=\" !selDate.sellPrice && info.supplierCode == 'PARTY_ZWY' \">\r\n                      <span class='ft50 bold'>{{ info.travelPrice/100 }}</span><span class=\"ft22\">起</span>\r\n                    </template>\r\n                    <template v-else>\r\n                      <span class='ft50 bold'>{{ info.sellPrice/100 }}</span>\r\n                    </template>\r\n                  </span>\r\n                  <span class=\"limit-text\">仅限会员</span>\r\n                </template>\r\n              </template>\r\n        \r\n              <!-- 都可购买 -->\r\n              <template v-if=\"info.buyLimit == 1\">\r\n                <span v-if=\"!info.sellPrice && !info.noVipPrice\" class='ft50 bold cFFD'>限时免费</span>\r\n                <template v-else>\r\n                  <!-- 自我游卡券显示价格区间 -->\r\n                  <template v-if=\" !selDate.sellPrice && info.supplierCode == 'PARTY_ZWY'\">\r\n                      <span class=\"cFFD ft28\">¥</span>\r\n                      <span class='ft50 bold cFFD'>{{ info.travelPrice/100 }}<span class=\"ft22\">起</span></span>\r\n                  </template>\r\n                  <template v-else>\r\n                    <span class='cFFD ft28'>¥<span class='ft50 bold'>{{info.sellPrice/100}}</span></span>\r\n                    <span class=\"limit-text\">非会员¥{{info.noVipPrice/100}}</span>\r\n                  </template>\r\n                </template>\r\n              </template> \r\n            </div>\r\n          </template>\r\n          <!-- 活动状态 1：未发布；2：进行中；3：暂停；4：结束； -->\r\n          <div class=\"flex-v\" @click=\"handlerEvent\">\r\n            <div class=\"tac submit-btn\">\r\n              <button class=\" ft34 tac submit bold\" :class=\"btnDisabled ? 'cff' : 'c8C4'\"  id=\"STATIS_BUY\">{{btnText}}</button>\r\n              <template v-if=\" !btnDisabled && selCashred.money && (info.sellPrice || info.noVipPrice) && info.channelType != 'CHANNEL_CONSUMER_COUPON' && info.channelType != 'CHANNEL_FORTUNE_COUPON'\">\r\n                <!-- 显示会员券后价 -->\r\n                <div  class=\"ft26 c8C4\" v-if=\"(info.buyLimit == 1 && vipUser) || info.buyLimit == 2\">券后价¥{{ (info.sellPrice - selCashred.money) > 0 ? (((info.sellPrice - selCashred.money)/100) | formatFloat ): 0 }}</div>\r\n                <!-- 显示非会员券后价 -->\r\n                <div  class=\"ft26 c8C4\" v-else-if=\"info.buyLimit == 1\">券后价¥{{ (info.noVipPrice - selCashred.money) > 0 ? (((info.noVipPrice - selCashred.money)/100) | formatFloat) : 0 }}</div>\r\n              </template>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </template>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    name: 'activeFooter',\r\n    components: {},\r\n    props: {\r\n      selDate: {\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      },\r\n      info: {\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      },\r\n      activeType: {  // 0 限时抢购 1 限量抢购\r\n        type: Number,\r\n        default: 0\r\n      },\r\n      btnDisabled: {\r\n        type: Boolean,\r\n        default: false\r\n      },\r\n      selCashred: {\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      },\r\n      btnText: {\r\n        type: String,\r\n        value: \"\"\r\n      },\r\n      selConsumer: {\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        cashRedList: [],\r\n        cashMask: false,\r\n        isIos: false,\r\n        vipUser: {}\r\n      }\r\n    },\r\n    methods: {\r\n      handlerEvent(e) {\r\n        this.$emit('success', e)\r\n      },\r\n      remindSet(e) {\r\n        this.$emit('remindSet', e)\r\n      }\r\n    },\r\n    created() {\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n  .mg4{\r\n    margin: 0 4px 0 0;\r\n  }\r\n  .mr18{\r\n    margin-right: 18px;\r\n  }\r\n  .ml20 {\r\n    margin-left: 20px;\r\n  }\r\n  .footer-text{\r\n    width: 100%;\r\n    height: 100%;\r\n    bottom: 0;\r\n    left: 0;\r\n    padding-left: 40px;\r\n    box-sizing: border-box;\r\n  }\r\n  .submit{\r\n    width: 250px;\r\n    line-height: 100px;\r\n    background: transparent;\r\n  }\r\n  .submit-btn {\r\n    width: 250px;\r\n  }\r\n  .submit-btn button {\r\n    line-height: 34px;\r\n  }\r\n  .submit-btn div {\r\n    line-height: 42px;\r\n  }\r\n  .limit-text{\r\n    opacity: 0.6;\r\n    font-size: 26px;\r\n    color: #FFFFFF;\r\n    margin-left: 16px;\r\n  }\r\n  .footer_no {\r\n    font-size: 34px;\r\n    font-weight: bold;\r\n    color: white;\r\n    width: 100%;\r\n    height: 100px;\r\n    background-image: linear-gradient(270deg, #474C61 0%, #222532 100%);\r\n  }\r\n  // 限时限量\r\n  .skill_footer {\r\n    width: 100%;\r\n    position: fixed;\r\n    left: 0;\r\n    bottom: 0;\r\n    z-index: 3;\r\n  }\r\n  .footer_btn {\r\n    padding: 0 50px 0 40px;\r\n    box-sizing: border-box;\r\n    height: 100px;\r\n  }\r\n  .footer_yellow {\r\n    background: url('http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/button_02.png') no-repeat;\r\n    background-size: 100% 100px;\r\n  }\r\n  .footer_red {\r\n    background: url('http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/button_01.png') no-repeat;\r\n    background-size: 100% 100px;\r\n  }\r\n  .footer_gray {\r\n    background: url('http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/button_03.png') no-repeat;\r\n    background-size: 100% 100px;\r\n  }\r\n  .footer_no {\r\n    font-size: 34px;\r\n    font-weight: bold;\r\n    color: white;\r\n    width: 100%;\r\n    height: 100px;\r\n    background-image: linear-gradient(270deg, #474C61 0%, #222532 100%);\r\n  }\r\n  .footer_left {\r\n    color: #94959C;\r\n  }\r\n  .footer_right {\r\n    font-size: 34px;\r\n    color: white;\r\n  }\r\n  .ft54 {\r\n    font-size: 54px;\r\n  }\r\n  .color_y {\r\n    color: #FFD683;\r\n  }\r\n\r\n  .mr20 {\r\n    margin-right: 20px;\r\n  }\r\n  .footer_tips {\r\n    width: 100%;\r\n    height: 64px;\r\n    opacity: 0.8;\r\n    background: #333333;\r\n    font-size: 26px;\r\n    color: #FFFFFF;\r\n  }\r\n  .footer_buyrule {\r\n    opacity: 0.7;\r\n    font-size: 26px;\r\n    color: #FFFFFF;\r\n    margin-left: 20px;\r\n  }\r\n</style>"]}]}