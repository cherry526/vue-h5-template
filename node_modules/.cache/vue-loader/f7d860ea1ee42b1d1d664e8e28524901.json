29:24\r\n--> \r\n<template>\r\n  <div>\r\n    <div class=\"active-panel ft26\">\r\n      <div class=\"flex-v gift-title\">\r\n        <span class=\"bold\">礼包详情</span>\r\n        <div class=\"ft24 ml12\">\r\n          <span class='c9E'>/包含<span v-if=\"info.showSendVip\"><span class=\"cF51\">1</span>张会员卡, </span><span class=\"cF51\">{{info.cardSum}}</span>张卡券</span>\r\n        </div>\r\n      </div>\r\n      <!-- 会员卡信息 -->\r\n      <div class='w flex-start bold gift-card-item' v-if=\"info.showSendVip\" @click=\"toEquity\">\r\n        <img class=\"vip-logo\" src=\"https://oss.kdwaimai.com/jkb/1582784612206_880972.png\">\r\n        <div>\r\n          <div class=\"vip-title\">潮级VIP</div>\r\n          <div class=\"vip-card\">\r\n            <div class='ft40 name'> {{info.vipCard.vipCardName}}</div>\r\n            <div class='ft26 time'>激活后，{{info.vipCard.vaildDay}}天内有效</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 卡券信息 -->\r\n      <!-- v-if='{{index < 4}}'  -->\r\n      <div class='w gift-coup-item flex-start' v-for=\"(item, index) in info.cardList\" :key=\"index\"  @click=\"toCouponDetail(item)\">\r\n        <img :src='item.shopPic' class='vip-logo mr30'>\r\n        <div>\r\n          <div class='vip-title'>{{item.shopName}}</div>\r\n          <div class='por coup-item-card ft24 lh1'>\r\n            <div class='coup-num poa tac' v-if=\"item.cardNum > 0\">含{{item.cardNum}}张券</div>\r\n            <div class='ft28 bold'>{{item.cardName}}</div>\r\n            <div class='coup-op coup-shop'>\r\n              <template v-if=\"(item.cardType == 'CARD_ITEM_COUPON' || item.cardType == 'CARD_MULTI_COUPON') && item.subscribeState != 2 \">\r\n                <span v-if=\" item.subscribeState == 0\">免预约</span>\r\n                <span v-else>提前{{item.appointmentTime}}{{item.timeType == 1 ? '小时' : '天'}}预约</span>\r\n              </template>\r\n              {{item.suitShop || ''}}\r\n            </div>\r\n            <div class='coup-op'>\r\n              <span class=''>{{item.limitTimeInterval || '营业时间可用'}} | {{item.limitFunds ? '每满' + item.limitFunds/100 + '元可用1张' : '无门槛使用'}} | {{item.stackable ? '单次可用' + item.limitStackable + '张' : '不限使用张数'}}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- <div class='more-recom cD9 tac' v-if='{{info.cardList.length > 4}}' @click='giftlookMore'>查看更多</div> -->\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    name: 'activeGift',\r\n    components: {},\r\n    props: {\r\n      info: { // 活动信息\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      },\r\n      channelType: {\r\n        type: String,\r\n        default: ''\r\n      },\r\n      options: {\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n      }\r\n    },\r\n    methods: {\r\n      toEquity() {\r\n        // wx.navigateTo({\r\n        //   url: '/pages/center/join_vip/join_vip'\r\n        // });\r\n      },\r\n      toCouponDetail(item) {\r\n        this.$router.push({name: 'activeCoupon', query: {channelType: this.channelType, cardId: item.cardId, detailId: item.detailId, actSn: this.options.actSn}})\r\n      },\r\n    },\r\n    created() {\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n  .ml12 {\r\n    margin-left: 12px;\r\n  }\r\n  .gift-title{\r\n    font-size: 32px;\r\n    color: #1A1919;\r\n    margin-bottom: 30px;\r\n    margin-right: 12px;\r\n  }\r\n  /* 礼包卡券详情 */\r\n  .gift-card-item, .gift-coup-item {\r\n    margin-bottom: 48px;\r\n  }\r\n  .gift-panel .gift-coup-item:last-of-type {\r\n    margin-bottom: 0;\r\n  }\r\n  .vip-card{\r\n    width: 612px;\r\n    height: 172px;\r\n    background: url('http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/bg_lib_card.png') no-repeat center;\r\n    background-size: 612px 172px;\r\n    padding: 44px 0 0 30px;\r\n    color: #FFCFA5;\r\n  }\r\n  .vip-logo {\r\n    width: 72px;\r\n    height: 72px;\r\n    margin-right: 18px;\r\n    border-radius: 50%;\r\n  }\r\n  .vip-card .name{\r\n    margin-bottom: 18px;\r\n  }\r\n  .vip-card .time{\r\n    opacity: 0.8;\r\n    font-weight: normal;\r\n  }\r\n  .vip-title {\r\n    font-size: 28px;\r\n    line-height: 28px;\r\n    margin-bottom: 18px;\r\n    padding-top: 8px;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .coup-item-card{\r\n    width: 612px;\r\n    height: 172px;\r\n    background: url('http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/bg_lib_quan.png') no-repeat center;\r\n    background-size: 612px 172px;\r\n    padding: 30px 0 0 30px;\r\n    color: #8C4B00;\r\n  }\r\n  .coup-shop {\r\n    margin: 18px 0;\r\n  }\r\n  .coup-op {\r\n    opacity: 0.8;\r\n  }\r\n  .coup-num{\r\n    right: 0;\r\n    top: 0;\r\n    background-image: linear-gradient(90deg, #FFE6B7 0%, #FFE6B7 0%, #FFCF94 100%);\r\n    border-radius: 0 12px 0 14px;\r\n    font-size: 22px;\r\n    height: 40px;\r\n    line-height: 40px;\r\n    padding: 0 10px;\r\n    box-sizing: border-box;\r\n  }\r\n</style>"]}]}