tem'\r\n\r\n  export default {\r\n    name: '',\r\n    mixins: [],\r\n    components:{\r\n      shopAddressItem\r\n    },\r\n    data() {\r\n      return {\r\n        suitShops: []\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapGetters([\"locationInfo\"])\r\n    },\r\n    methods: {\r\n      //适用门店列表\r\n      getSuitShops(cardId, detailId) {\r\n        this.$api.suitShop({\r\n          cardId,\r\n          detailId,\r\n          lat: this.locationInfo.latitude,\r\n          lng: this.locationInfo.longitude,\r\n        }).then(res => {\r\n          if (res.state) {\r\n            this.suitShops = res.info ? (res.info.suitShops || []) : []\r\n          }\r\n        })\r\n      },\r\n    },\r\n    created() {\r\n      if (this.$route.query.type == 1) { // 卡券详情页进入\r\n        this.suitShops = this.$save.get('session', 'applystore')\r\n      } else {\r\n        this.getSuitShops(this.$route.query.cardId, this.$route.query.detailId)\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style  lang='scss'>\r\n  .address-list {\r\n    padding-left: 30px;\r\n    padding-top: 20px;\r\n    background: #fff;\r\n  }\r\n  .address-list .sn-item{\r\n    border-bottom: 1px solid #E5E6E8;\r\n  }\r\n  .address-list .sn-item:last-of-type{\r\n    border: none;\r\n  }\r\n</style>"]}]}