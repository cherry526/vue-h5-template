0:22:16\r\n--> \r\n<template>\r\n  <div>\r\n    <template  v-if=\"info.supplierCode != 'PARTY_ZWY' && info.cardType != 'CARD_DIRECT_RECHARGE'\">\r\n      <div class=\"active-panel\">\r\n        <!-- 品牌 -->\r\n        <div class=\"shop-brand\">\r\n          <div class='flex-c-b w'>\r\n            <span class='ft32 bold flex-1'>品牌门店</span>\r\n            <div @click.stop=\"toApply\" v-if=\"suitShops.length > 1 && info.shopShow\">\r\n              <span class='ft24 c66' >{{ suitShops.length }}家门店适用</span>\r\n              <span class=\"iconfont iconicon_more c9E\"></span>\r\n            </div>\r\n          </div>\r\n          <div class=\"flex-v shop-desc\" >\r\n            <img class=\"shop-pic\" :src='info.shopPic'>\r\n            <div class=\"flex-1 shop-right\">\r\n              <div class=\"ft26 bold\">{{ info.shopName }}</div>\r\n              <div class='shop-label ft22 flex-wrap'> \r\n                <span  v-for='(item, index) in lables' :key=\"index\">{{item}}</span> \r\n              </div> \r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- 门店 -->\r\n        <div class='apply-store' @click=\"_toShopDetail(suitShops[0].shopSn)\" v-if=\"info.shopShow && suitShops.length\">\r\n          <div class='ft26 c1A'>{{ suitShops[0].shopName }}</div>\r\n          <div class=\"flex-c-b\">\r\n            <div class=\"flex-v mt16\">\r\n              <div class=\"flex-v hot cCD ft24\">\r\n                <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/icon_hot.png\">\r\n                人气{{ change_detail.visitCnt }}\r\n              </div>\r\n              <div class='ft24 c9E' v-if=\"fixedDetail.perCapita\">人均¥{{ fixedDetail.perCapita || 0}}</div>\r\n            </div>\r\n             <a class=\"iconfont iconicon_tel icon_phone\" :href=\"'tel:' + suitShops[0].tel\"></a>\r\n          </div>\r\n      \r\n          <div class='label-box flex-c-b'>\r\n            <div class=\"flex-v\">\r\n              <span class=\"iconfont iconicon_address icon\"></span>\r\n              <div class=\"t-elps dis-span\">\r\n                <span class='ft24 c9E'>{{ suitShops[0].distance | formatDistance}} | </span>\r\n                <span class='c9E'>{{ suitShops[0].address }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </template>\r\n  </div>\r\n</template>\r\n<script>\r\n  import { mapGetters } from \"vuex\";\r\n\r\n  export default {\r\n    name: 'activeBrand',\r\n    components: {},\r\n    props: {\r\n      suitShops: {\r\n        type: Array\r\n      },\r\n      info: {\r\n        type: Object\r\n      },\r\n      options: {\r\n        type: Object\r\n      },\r\n      fixedDetail: {\r\n        type: Object\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        lables: '',\r\n        shopSn: '',\r\n        change_detail: {}\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapGetters([\"locationInfo\"]),\r\n    },\r\n    watch: {\r\n      suitShops: {\r\n        deep: true,\r\n        immediate: true,\r\n        handler: function (val, oldValue) {\r\n          console.log(val, 'val')\r\n          if (val.length) {\r\n            this.shopSn = val[0].shopSn\r\n            this.getChangeDeil()\r\n          }\r\n        },\r\n      }\r\n    },\r\n    methods: {\r\n      // 查看门店详情\r\n      _toShopDetail(e) {\r\n        // wx.navigateTo({\r\n        //   url: `/pages/index/shop_detail/shop_detail?shopSn=${e.currentTarget.dataset.shopsn}`\r\n        // });\r\n      },\r\n      // 查看适用门店\r\n      toApply() {\r\n        this.$router.push({\r\n          name:'applyAddressShop',\r\n          query: {\r\n            cardId: this.options.cardId || this.info.cardId,\r\n            detailId: this.options.detailId || this.info.detailId\r\n          }\r\n        })\r\n      },\r\n      // 获取商家变动信息\r\n      getChangeDeil() {\r\n        this.$api.shop_change_detail({\r\n          shopSn: this.shopSn\r\n        }).then(res => {\r\n          if (res.state) {\r\n            this.change_detail = res.info\r\n          }\r\n        })\r\n      }\r\n    },\r\n    created() {\r\n      this.lables = this.info.lables ? this.info.lables.split(',') : []\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n  .shop-pic{\r\n    width: 108px;\r\n    height: 108px;\r\n    border: 1px solid #EBE9E6;\r\n    border-radius: 6px;\r\n    margin-right: 13px;\r\n  }\r\n\r\n  .shop-label{\r\n    margin-top: 18px;\r\n  }\r\n  .shop-label span{\r\n    padding: 7px 12px;\r\n    background: #FEEED5;\r\n    border-radius: 3px;\r\n    color: #CD7B00;\r\n    margin-right: 12px;\r\n    margin-bottom: 12px;\r\n  }\r\n  .shop-desc {\r\n    padding: 24px 0 0 0;\r\n    box-sizing: border-box;\r\n  }\r\n  .shop-brand .iconicon_more {\r\n    font-size: 20px;\r\n  }\r\n\r\n  .apply-store{\r\n    border-top: 2px solid rgba(245, 243, 240, 0.8); \r\n    padding: 24px 10px 0 0;\r\n    margin-top: 24px;\r\n    box-sizing: border-box;\r\n  }\r\n  .label-box{\r\n    font-size: 24px;\r\n    color: #9E9D9B;\r\n    margin-top: 12px;\r\n  }\r\n  .apply-store .icon{\r\n    font-size: 30px;\r\n    margin-right: 6px;\r\n    color: #B3B1AF;\r\n  }\r\n  .icon_phone{\r\n    color: #666564;\r\n    font-size: 34px;\r\n    padding-left: 36px; \r\n    border-left: 1px solid rgb(158, 157, 155, 0.2);\r\n  }\r\n  .dis-text{\r\n    width: 586px;\r\n    padding-right: 39px;\r\n    line-height: 30px;\r\n  }\r\n  .active-panel .hot {\r\n    margin-right: 18px;\r\n  }\r\n  .active-panel .hot img {\r\n    width: 20px;\r\n    height: 24px;\r\n    margin-right: 8px;\r\n  }\r\n\r\n</style>"]}]}