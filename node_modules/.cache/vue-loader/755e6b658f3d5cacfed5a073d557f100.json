{"remainingRequest":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\src\\views\\active\\travelInformation.vue?vue&type=style&index=0&lang=scss&","dependencies":[{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\src\\views\\active\\travelInformation.vue","mtime":1597889072586},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\sass-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Desktop\\vip_inline_h5\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["travelInformation.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkeA;AACA;AACA;AACA;AACA;AACA","file":"travelInformation.vue","sourceRoot":"src/views/active","sourcesContent":["<!--\r\n * @Description: 活动填写用户信息\r\n * @Version: 1.0\r\n * @Autor: cherry\r\n * @Date: 2020-07-31 10:33:11\r\n * @LastEditors: yesong\r\n * @LastEditTime: 2020-08-12 19:03:18\r\n--> \r\n<template>\r\n  <div class=\"information-page\">\r\n    <div class=\"controller\" v-if=\"showCalendar\">\r\n      <div class=\"content pl24\">\r\n        <div class=\"input-item flex-v\" @click=\"openCalendar\">\r\n          <div class=\"label ft32 c1A\">游玩日期<span class=\"cD9\">*</span></div>\r\n          <div class=\"flex-c-b flex-1\">\r\n            <span class=\"ft26\" v-if=\"selDate.startTime && info.validityType == 3\">已选{{selDate.startTime | formatTime('YYYY/MM/DD') }}</span>\r\n            <span class=\"ft26\" v-else-if=\"selDate.startTime\">已选游玩日:{{ selDate.startTime | formatTime('YYYY/MM/DD')}} ~ {{ selDate.endTime | formatTime('YYYY/MM/DD') }}</span>\r\n            <span class=\"ft32 c9E\" v-else style=\"color: #C7C5C3;\">请选择游玩日期</span>\r\n            <span class=\"iconfont iconicon_more ft18\" style=\"color: #C7C5C3; margin-right: 30px;\"></span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n      <div class=\"controller\" >\r\n        <div class=\"title ft24 c66\">购物此类商品，须填写以下信息</div>\r\n        <div class=\"content pl24\" :style=\"index >= 1 ? 'margin-top: 18px' : ''\" v-for=\"(item,index) in formList\" :key=\"index\">\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowName\">\r\n            <div class=\"label ft32 c1A\">用户名<span class=\"cD9\" v-if=\"item.isFullName\">*</span></div>\r\n            <input class=\"flex-1\" type=\"text\" :name=\"'userName' + index\" v-model=\"item.userName\" placeholder=\"请输入用户名\">\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowIdentity\">\r\n            <div class=\"label ft32 c1A\">身份证<span class=\"cD9\" v-if=\"item.isFullIdentity\">*</span></div>\r\n            <input class=\"flex-1\" :name=\"'idCard' + index\" type=\"text\" v-model=\"item.idCard\" placeholder=\"请输入身份证\">\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowPhone\">\r\n            <div class=\"label ft32 c1A\">手机号码<span class=\"cD9\" v-if=\"item.isFullPhone\">*</span></div>\r\n            <input class=\"flex-1\" :name=\"'phone' + index\" v-model=\"item.phone\" type=\"number\" placeholder=\"请输入手机号码\">\r\n            <button v-if=\"!wxUser.phone && index == 0\" class=\"auth-phone\">授权</button>\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowEmial\">\r\n            <div class=\"label ft32 c1A\">邮箱<span class=\"cD9\" v-if=\"item.isfullEmial\">*</span></div>\r\n            <input class=\"flex-1\" type=\"text\" :name=\"'email' + index\" v-model=\"item.email\" placeholder=\"请输入邮箱\">\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowAdress\" >\r\n            <div class=\"label ft32 c1A\">地址<span class=\"cD9\" v-if=\"item.isFullAdress\">*</span></div>\r\n            <div class=\"flex-c-b flex-1\" @click=\"addressPicker(cityList[index], index)\">\r\n              <span class=\"ft32 c1A\" v-if=\"cityList[index] && cityList[index].length > 0\">{{ cityList[index][0] }}</span>\r\n              <span class=\"ft32\" style=\"color: #C7C5C3;\" v-else >请选择地址</span>\r\n              <span class=\"iconfont iconicon_more ft18\" style=\"color: #C7C5C3; margin-right: 30px;\"></span>\r\n            </div>\r\n\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowAdress\">\r\n            <div class=\"label ft32 c1A\">详细地址<span class=\"cD9\" v-if=\"item.isFullAdress\">*</span></div>\r\n            <input :name=\"'addressDetail' + index\" type=\"text\" v-model=\"item.addressDetail\" placeholder=\"请输入详细地址\">\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n\r\n      <!-- 备注 -->\r\n      <div class=\"controller\" v-if=\"info.receiveData && info.receiveData.isShowRemarks\">\r\n        <div class=\"content pl24 remark\">\r\n          <div class=\"input-item flex\">\r\n            <div class=\"label ft32 c1A\">备注<span class=\"cD9\" v-if=\"info.receiveData.isfullRemark\">*</span></div>\r\n            <textarea name=\"remark\" v-model=\"formList[0].remark\" placeholder=\"填写特殊情况和需求\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 紧急联系人 -->\r\n      <div class=\"controller\" v-if=\"info.urgentContact\">\r\n        <div class=\"title ft24 c66\">紧急联系人</div>\r\n        <div class=\"content pl24\">\r\n          <div class=\"input-item flex-v\">\r\n            <div class=\"label ft32 c1A\">联系人<span class=\"cD9\">*</span></div>\r\n            <input class=\"flex-1\" name=\"emergencyContact\" type=\"text\" v-model=\"formList[0].emergencyContact\" placeholder=\"请输入紧急联系人姓名\">\r\n          </div>\r\n          <div class=\"input-item flex-v\">\r\n            <div class=\"label ft32 c1A\">手机号码<span class=\"cD9\">*</span></div>\r\n            <input class=\"flex-1\" name=\"emergencyPhone\" type=\"number\" v-model=\"formList[0].emergencyPhone\" placeholder=\"请输入紧急联系人手机号码\">\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"btn\" @click=\"formSubmit\">\r\n        <img class=\"bg-img\" src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/travel_information_btn.png\">\r\n        <button class=\"bold\" type=\"submit\">确定</button>\r\n      </div>\r\n      \r\n      \r\n      <van-popup v-model=\"showPicker\" round position=\"bottom\">\r\n        <van-area \r\n          title=\"请选择地址\"\r\n          value=\"110101\"\r\n          @confirm=\"confirmPicker\"\r\n          @cancel=\"cancelPicker\"\r\n          :area-list=\"areaList\"  />\r\n      </van-popup>\r\n\r\n    <!-- 日历弹窗 -->\r\n    <active-calendar ref=\"calendar\" v-if=\"showCalendar\"  @travelMonthInfo=\"getTravelMonthInfo\" :info=\"info\" :actSn=\"actSn\" :calendarMask.sync=\"calendarMask\" :channelType=\"channelType\"></active-calendar>\r\n  </div>\r\n</template>\r\n<script>\r\n  import { mapGetters, mapActions, mapMutations } from \"vuex\";\r\n  import { Area }  from 'vant'\r\n  import address from '@/plugins/address'\r\n  import utils from '@utils/utils'\r\n  import activeCalendar from '@components/active/activeCalendar'\r\n\r\n  export default {\r\n    name: 'travelInformation',\r\n    mixins: [],\r\n    computed: {\r\n      ...mapGetters([\"wxUser\"])\r\n    },\r\n    components:{\r\n      VanArea: Area,\r\n      activeCalendar\r\n    },\r\n    data() {\r\n      return {\r\n        currentAddress: 0, \r\n        areaList: address,\r\n        showPicker: false,\r\n        info: {},\r\n        cityList: [[]],\r\n        formList: [{\r\n          userName: '',\r\n          idCard: '',\r\n          phone: '',\r\n          email: '',\r\n          addressDetail: '',\r\n          city: [],\r\n          remark: '',\r\n          emergencyContact: '',\r\n          emergencyPhone: ''\r\n        }],\r\n        calendarMask: false,\r\n        selDate: {\r\n          startTime: '',\r\n          endTime: ''\r\n        }, // 自我游选中的时间\r\n        showCalendar: false,\r\n        showCalendarPro: false,\r\n        isShowLsit: [{\r\n          isShow: 'isShowName',\r\n          isEach: 'isEachPeopleNameWrite',\r\n          isFull: 'isFullName',\r\n          value: 'userName',\r\n          message: '用户名不能为空'\r\n        }, {\r\n          isShow: 'isShowPhone',\r\n          isEach: 'isEachPeoplePhoneWriter',\r\n          isFull: 'isFullPhone',\r\n          value: 'phone',\r\n          message: '手机号码不能为空'\r\n        }, {\r\n          isShow: 'isShowIdentity',\r\n          isEach: 'isEachPeopleIdentityWriter',\r\n          isFull: 'isFullIdentity',\r\n          value: 'idCard',\r\n          message: '身份证不能为空'\r\n        }, {\r\n          isShow: 'isShowEmial',\r\n          isEach: 'isEachPeopleEmialWriter',\r\n          isFull: 'isfullEmial',\r\n          value: 'email',\r\n          message: '邮箱不能为空'\r\n        }, {\r\n          isShow: 'isShowAdress',\r\n          isEach: 'isEachPeopleAdressWriter',\r\n          isFull: 'isFullAdress',\r\n        },\r\n        {\r\n          isFull: 'isfullRemark',\r\n          isShow: 'isShowRemarks',\r\n          value: 'remark',\r\n          message: '备注不能为空'\r\n        }]\r\n      }\r\n    },\r\n    methods: {\r\n      //\r\n      cancelPicker() {\r\n        this.showPicker = false\r\n      },\r\n      // \r\n      confirmPicker(e) {\r\n        let address = e.reduce((pre, cur) => {\r\n          return pre + `${cur.name} `\r\n        }, '')\r\n        this.$set(this.cityList, this.currentAddress, [address] )\r\n        this.cancelPicker()\r\n        console.log(this.cityList, 'this.cityList')\r\n      },\r\n      // 设置选中时间\r\n      setTime () {\r\n        let selDate= this.selDate\r\n        let date = new Date(selDate.startTime)\r\n        let _year = date.getFullYear() + '';\r\n        let _month = this.setString(date.getMonth() + 1);\r\n        let _day = this.setString(date.getDate())\r\n        let dateObj = {\r\n          span: _year + _month,\r\n          month: _month,\r\n          year: _year,\r\n          day: _day\r\n        }\r\n        console.log(dateObj)\r\n        this.$refs.calendar.showCalendarData(dateObj)\r\n\r\n      },\r\n\r\n      getTravelMonthInfo (detail) {\r\n        // 接收日历信息\r\n        if (detail.dataType == 'selDate' && this.calendarMask){\r\n          // 选择日历信息\r\n          let info = this.info\r\n          info.sellPrice = detail.sellPrice\r\n          info.noVipPrice = detail.noVipPrice\r\n          this.info = info\r\n          this.selDate = detail\r\n        }\r\n        \r\n      },\r\n\r\n      setString (span) {\r\n        return span > 9 ? span : '0'+span\r\n      },\r\n\r\n      openCalendar() {\r\n        this.calendarMask = true\r\n        this.setTime()\r\n      },\r\n\r\n      getInfo () {\r\n        this.$api.receive_condition({\r\n          actSn: this.options.actSn\r\n        }).then(res => {\r\n          if (res.state) {\r\n            res.info.receiveData = JSON.parse(res.info.receiveData)\r\n            res.info.worth = this.options.worth\r\n            this.info = res.info\r\n            console.log(this.info, '000000000000info')\r\n            if (res.info.conditionAsk == 1) {\r\n              // 需要每个人填写要求\r\n              this.setFormInfo(this.options.num)\r\n            } else {\r\n              this.setFormInfo(1)\r\n            }\r\n            if ([3, 4, 5].includes(res.info.validityType) && res.info.supplierCode == 'PARTY_ZWY') {\r\n              // 是否选择日期\r\n              this.showCalendar = true\r\n            }\r\n          }\r\n        })\r\n      },\r\n\r\n      setFormInfo (num) {\r\n        let travelInformation = this.$save.get('session', 'travelInformation') || {}\r\n        let formList = travelInformation.backList || []\r\n        let receiveData = this.info.receiveData\r\n        let userName = this.$save.get('session', 'travelInformationUserName') || ''\r\n\r\n        // 是否显示\r\n        let isShowLsit = this.isShowLsit\r\n\r\n        let formListNew = []\r\n        let cityList = []\r\n        for (let i=0;i < num;i++) {\r\n          if (formList[i]) {\r\n            formListNew.push(formList[i])\r\n            cityList.push(formList[i].city)\r\n          } else {\r\n            let formListNewObj = {\r\n              remark: '',\r\n              emergencyContact: '',\r\n              emergencyPhone: ''\r\n            }\r\n            if (i == 0) { // 第一个不用校验每个客人都需要输入\r\n              isShowLsit.map(item => { // 判断是否需要填写\r\n                if (receiveData[item.isShow]) {\r\n                  formListNewObj[item.isShow] = true\r\n                  if (item.isShow == 'isShowPhone') {\r\n                    formListNewObj.phone = this.wxUser.phone || ''\r\n                  }\r\n                  if (item.isShow == 'isShowName') {\r\n                    formListNewObj.userName = userName\r\n                  }\r\n                }\r\n                if (receiveData[item.isShow] && item.isShow === 'isShowAdress') {\r\n                  cityList.push([])\r\n                }\r\n                if (receiveData[item.isFull]) {\r\n                  formListNewObj[item.isFull] = true\r\n                }\r\n              })\r\n            } else {\r\n\r\n              isShowLsit.map(item => { // 判断需要每个用户填写\r\n                if (receiveData[item.isEach] && item.isEach === 'isEachPeopleAdressWriter') { // 判断地址\r\n                  cityList.push([])\r\n                } \r\n                if (receiveData[item.isShow] && receiveData[item.isEach]) {\r\n                  formListNewObj[item.isShow] = true\r\n                  if (receiveData[item.isFull]) {\r\n                    formListNewObj[item.isFull] = true\r\n                  }\r\n                }\r\n                \r\n              })\r\n            }\r\n            formListNew.push(formListNewObj)\r\n          }\r\n          \r\n        }\r\n        console.log(cityList, 'cityListcityListcityList', formListNew)\r\n        this.formList = formListNew\r\n        this.cityList = cityList\r\n      },\r\n\r\n      formSubmit($event) {\r\n        console.log( this.formList, 'formList')\r\n        if (this.calendarMask) {\r\n          this.calendarMask = false\r\n          return\r\n        } \r\n        let formList = this.formList\r\n        let cityList = this.cityList\r\n        let valueKey = ['addressDetail', 'email', 'idCard', 'phone', 'userName']\r\n        for (let i=0; i<formList.length; i++) {\r\n          formList[i].idCard = formList[i].idCard ? formList[i].idCard : ''\r\n          formList[i].userName = formList[i].userName ? formList[i].userName : ''\r\n          formList[i].city = cityList[i] || []\r\n        } \r\n        this.rules(formList) // 过滤必填项\r\n      },\r\n\r\n      rules(formList) {\r\n        let info = this.info\r\n        let isRules = true\r\n        let rulesList = this.isShowLsit\r\n\r\n        for (let i = 0; i < formList.length; i++) {\r\n          rulesList.map(item => {\r\n            if (formList[i][item.isFull] && formList[i][item.isShow]) { // 判断必填项\r\n              if (item.isFull === 'isFullAdress') { // 判断地址\r\n                if (!formList[i].city) {\r\n                  this.$Toast('请选择地址')\r\n                  isRules = false\r\n                  return false\r\n                }\r\n                if (formList[i].addressDetail == '') {\r\n                  this.$Toast('请输入详细地址')\r\n                  isRules = false\r\n                  return false\r\n                }\r\n              }  else if (formList[i][item.value] == '' ) {\r\n                this.$Toast(item.message)\r\n                isRules = false\r\n                return false\r\n              }\r\n            }\r\n            if (item.isFull === 'isFullPhone' && formList[i].phone) {\r\n              if (!utils.verifyPhone(formList[i].phone)) {\r\n                 this.$Toast('手机号格式不正确')\r\n                isRules = false\r\n                return false\r\n              }\r\n            } else if (item.isFull === 'isfullEmial' && formList[i].email) {\r\n              if (!utils.verifyEmail(formList[i].email)) {\r\n                this.$Toast('邮箱格式不正确')\r\n                isRules = false\r\n                return false\r\n              }\r\n            } else if (item.isFull === 'isFullIdentity' && formList[i].idCard != '') {\r\n              console.log( formList[i], ' formList[i].idCard')\r\n              if (formList[i].idCard.length != 18) {\r\n                this.$Toast('身份证格式不正确')    \r\n                isRules = false\r\n                return false\r\n              } \r\n            }\r\n          })\r\n\r\n\r\n\r\n          if (formList[i].userName) {\r\n            if (formList[i].userName.length > 10) {\r\n              this.$Toast('用户名长度不能超过10位')\r\n              isRules = false\r\n              return false\r\n            }\r\n          }\r\n\r\n          if (info.urgentContact && (formList[i].emergencyContact == '' || formList[i].emergencyPhone == '')) {\r\n            this.$Toast('请输入紧急联系人信息')\r\n            isRules = false\r\n            return false\r\n          }\r\n\r\n          if (formList[i].emergencyPhone != '') {\r\n            if (!utils.verifyPhone(formList[i].emergencyPhone)) {\r\n              this.$Toast('紧急联系人手机号格式不正确')\r\n              isRules = false\r\n              return false\r\n            }\r\n          }\r\n\r\n          if (info.receiveData.isShowRemarks && info.receiveData.isfullRemark && formList[i].remark == '') {\r\n            this.$Toast('请填写备注信息')\r\n            isRules = false\r\n            return false\r\n          }\r\n          formList[i].address = formList[i].city[0] + formList[i].addressDetail\r\n\r\n          // formList[i].address = formList[i].city + formList[i].addressDetail\r\n        }\r\n\r\n        if (this.showCalendar && !this.selDate.startTime) {\r\n          // 需要选择日期时\r\n          isRules = false\r\n          this.$Toast('请选择日期')\r\n          this.calendarMask = true\r\n        }\r\n        if (isRules) {\r\n          this.setInfo()\r\n        }\r\n      },\r\n\r\n      setInfo () {\r\n        let formList = this.formList\r\n        let formListNew = [], isNull = true\r\n        let valueKey = ['address', 'email', 'idCard', 'phone', 'userName', 'emergencyContact', 'emergencyPhone', 'remark']\r\n        for (let i=0;i<formList.length; i++) {\r\n          let obj  = {}, isShow = false\r\n          for (let y = 0; y < valueKey.length;y++) {\r\n            if (formList[i][valueKey[y]]) {\r\n              isNull = false\r\n              isShow = true\r\n              obj[valueKey[y]] = formList[i][valueKey[y]]\r\n            }\r\n          }\r\n          if (isShow) {\r\n            formListNew.push(obj)\r\n          } \r\n        }\r\n        formListNew.length == 0 ? formListNew.push({}) : \"\"\r\n        let travelInformation = {\r\n          backList: this.formList, // 用于回显数据\r\n          formList: formListNew, // 用于提交请求\r\n          selDate: this.selDate,\r\n          isNull\r\n        }\r\n        console.log(travelInformation, 'travelInformation')\r\n        // 校验成功保存数据\r\n        this.$save.set('session', 'travelInformation', travelInformation)\r\n        this.$save.set('session', 'travelInformationUserName', travelInformation.formList[0].userName)\r\n        this.$router.go(-1)\r\n      },\r\n\r\n      addressPicker(value, index) {\r\n        this.showPicker = true\r\n        this.currentAddress = index\r\n      },\r\n    },\r\n    created() {\r\n      this.options = this.$route.query\r\n      this.options.num = this.options.num || 1\r\n      this.channelType = this.options.channelType\r\n      this.getInfo()\r\n      if (this.options.selDate) {\r\n        console.log(this.options.selDate, JSON.parse(this.options.selDate))\r\n        this.selDate = JSON.parse(this.options.selDate)\r\n        this.actSn = this.options.actSn\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style lang='scss'>\r\n  .information-page {\r\n    .calendar-mask {\r\n      bottom: 140px;\r\n      height: calc(100% - 140px);\r\n    }\r\n  } \r\n</style>\r\n\r\n<style scoped lang='scss'>\r\n.information-page {\r\n  padding-bottom: 180px;\r\n}\r\n.controller {\r\n  margin-top: 18px;\r\n}\r\n.controller .title {\r\n  padding: 12px 0 18px 24px;\r\n}\r\n.content {\r\n  background: #fff;\r\n}\r\n.pl24 {\r\n  padding-left: 24px;\r\n}\r\n.input-item {\r\n  height: 100px;\r\n  border-bottom: 1px solid #F6F6F6;\r\n}\r\n.input-item .label {\r\n  width: 180px;\r\n}\r\n.input-item input {\r\n  height: 100px;\r\n}\r\n.remark .input-item {\r\n  padding-top: 30px;\r\n  height: auto;\r\n}\r\n\r\n.input-item textarea {\r\n  height: 160px;\r\n  font-size: 32px;\r\n}\r\n\r\n.btn {\r\n  position: fixed;\r\n  bottom: 0;\r\n  width: 100%;\r\n  padding: 22px 30px;\r\n  background: #FFFFFF;\r\n  box-shadow: 0 -1px 0 0 #F5F3F0, 0 0 12px 0 rgba(142,86,35,0.09);\r\n  box-sizing: border-box;\r\n  z-index: 13;\r\n}\r\n.btn .bg-img {\r\n  position: absolute;\r\n  height: 96px;\r\n  width: 690px;\r\n  left: calc(50% - 345px);\r\n}\r\n.btn button {\r\n  position: relative;\r\n  width: 100%;\r\n