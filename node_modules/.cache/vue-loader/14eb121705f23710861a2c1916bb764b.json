=\"f-gift-vip-icon poa\" v-if=\"!item.sellPrice\">\r\n        <template v-else-if=\"item.buyLimit == 2\">\r\n          <!-- 会员专享标签 -->\r\n          <div class=\"gift-vip-text poa\">会员专享</div>\r\n        </template>\r\n        \r\n        <!-- 活动logo -->\r\n        <img class='act-logo' :src='item.actPic + \"?x-oss-process=image/resize,m_lfit,h_216,w_336\"'>\r\n\r\n        <!-- 限时抢购显示剩余数量 -->\r\n        <div class=\"remain-num\" v-if=\"limitedType == 'ACT_LIMITED_COUNT'\">\r\n          {{item.cycleRemainingCnt < 100 ? '仅': ''}}剩{{ item.cycleRemainingCnt }}份\r\n        </div>\r\n        <div class=\"apply-shop\" v-else-if=\"limitedType == 'ACT_LIMITED_TIME' && item.soldOut\">\r\n          已售{{item.soldOut || 0}}\r\n        </div>\r\n        <div class=\"apply-shop\" v-else-if=\"item.receiveNum\">\r\n          已售{{item.receiveNum || 0}}\r\n        </div>\r\n\r\n        <!-- 活动结束水印 -->\r\n        <template v-if=\"item.actType != 'ACT_ASSEMBLE'\">\r\n          <div class='sale-out poa flex-c flex-v' v-if=\"(item.actState != 2 && !limitedType) || (limitedType && item.state == 2 || item.state == 3)\">\r\n            <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/update/sale_over2.png\" class=\"sale-over\">\r\n          </div>\r\n        </template>\r\n\r\n      </div>\r\n      <div class='act-info'>\r\n        <!-- 拼团标签+名称 -->\r\n        <template v-if=\"styleType == 1 && item.actType === 'ACT_ASSEMBLE'\">\r\n          <div class='group-name text-one flex-v'>\r\n            <img class=\"group-icon\" src=\"https://toss.wbgj.cn/wehub_upload/15867689280001586768928004_243812.png\"></img>\r\n            <div class='name'>超值拼团</div>\r\n          </div>\r\n        </template>\r\n        <!-- 礼包活动不显示店铺名称 -->\r\n        <template v-else>\r\n          <div class='shop-name flex-v' v-if=\"item.actType != 'ACT_GIFT_PACKAGE' && item.actType != 'ACT_ASSEMBLE'\">\r\n            <div class='name t-elps'>{{item.shopName}}</div>\r\n          </div>\r\n        </template>\r\n        <template v-if=\"styleType == 1 && item.actType === 'ACT_ASSEMBLE'\">\r\n          <div class='card-name t-elps'>{{item.actName}}</div>\r\n        </template>\r\n        <template v-else>\r\n          <div v-if=\"item.actType == 'ACT_ASSEMBLE'\" class='group-name2 text-two'>{{item.actName}}</div>\r\n          <div v-else :class='item.actType == \"ACT_GIFT_PACKAGE\" ? \"gift-name\" : \"card-name t-elps\"'>{{item.actName}}</div>\r\n        </template>\r\n        <!-- 拼团 -->\r\n        <div class=\"price flex-c-b\" v-if=\"item.actType === 'ACT_ASSEMBLE'\">\r\n          <span class=\"ft20 cf51\"><template v-if=\"styleType != 1 \">参团价</template>¥<span class=\"ft36 bold\">{{item.sellPrice/100}}</span></span>\r\n        </div>\r\n        <!-- 特卖、自我游、 限时限量价格显示 -->\r\n        <div class=\"price flex-c-b\" v-else>\r\n          <span v-if=\"item.supplierCode == 'PARTY_ZWY'\" class='c8C'><span class='ft20 cf51'>¥</span><span class=\"cf51 ft36 bold\">{{(item.sellPrice || 0) / 100}}</span><span class=\"ft22\"> 起</span></span>\r\n          <span v-else-if=\"(item.childEnumKey == 'ACT_SPECIAL_OFFER_PURCHASE_LIMIT' || item.childEnumKey == 'ACT_GIFT_PACKAGE_PURCHASE_LIMIT')\" class='c8C'><span class='ft20 cf51'>{{ limitedType == 'ACT_LIMITED_COUNT' ? '秒杀价' : '抢购价' }}¥</span><span class=\"cf51 ft36 bold\">{{(item.panicBuyingPrice || 0) / 100}}</span></span>\r\n          <span v-else class=\"ft20 cf51\">¥<span class=\"ft36 bold\">{{item.sellPrice/100}}</span></span>\r\n          <!-- 分销金额 -->\r\n          <!-- <div class=\"distributor-price flex-v\" v-if=\"item.promotion >0 && wxUser.distributorFlag\">\r\n            <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.5.6/label_zhuan.png\">\r\n            <span class=\"ft20\">{{ (item.promotion)/100 }}元</span>\r\n          </div> -->\r\n        </div>\r\n        <div class=\"flex-b ft22 c8C act-footer\">\r\n          <span class='' v-if=\"item.actType === 'ACT_ASSEMBLE' && styleType != 1\">原价¥{{item.originalPrice/100}}</span>\r\n          <span class='' v-else>原价¥{{item.worth/100}}</span>\r\n          <!-- 限时抢购不显示已售数量 -->\r\n          <div class=\"group-label\" v-if=\"item.actType === 'ACT_ASSEMBLE' && styleType == 2\">{{item.needPeople}}人团</div>\r\n          <span class='' v-if=\"(item.childEnumKey == 'ACT_SPECIAL_OFFER_PURCHASE_LIMIT' || item.childEnumKey == 'ACT_GIFT_PACKAGE_PURCHASE_LIMIT') && limitedType == 'ACT_LIMITED_TIME'\"><!-- 已售{{item.soldOut || 0}} -->{{ item.suitShop || ''}}</span>\r\n          <template v-if=\"!limitedType\">\r\n            <template v-if=\"item.actType === 'ACT_ASSEMBLE' && styleType == 2\"></template>\r\n            <template v-else>\r\n              <span class=''>{{ item.suitShop || ''}}</span>\r\n            </template>\r\n          </template>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  <!-- </div> -->\r\n</template>\r\n<script>\r\n  import linkPage from '@/router/linkPage'\r\n  export default {\r\n    name: 'activeItem',\r\n    mixins: [],\r\n    computed: {},\r\n    props: {\r\n      styleType: { // 1 精选特卖样式 2首页样式\r\n        type: Number,\r\n        default: 1\r\n      },\r\n      item: {\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      },\r\n      channelType: {\r\n        type: String,\r\n        default: 'CHANNEL_OVERFLOW_SPECIAL_ACTIVITY'\r\n      },\r\n      param: { // 限时限量所需参数\r\n        type: Object,\r\n        default: function() {\r\n          return {}\r\n        }\r\n      },\r\n      \r\n      /**\r\n       * ACT_LIMITED_TIME 限时\r\n       * ACT_LIMITED_COUNT 限量\r\n       */\r\n      limitedType: {\r\n        type: String,\r\n        default: ''\r\n      }\r\n    },\r\n    components:{},\r\n    data() {\r\n      return {\r\n      }\r\n    },\r\n    methods: {\r\n      // 跳转活动\r\n      toActive() {\r\n        console.log('跳转活动')\r\n        if (this.limitedType) {\r\n          let type = this.param.enumkey === 'ACT_LIMITED_COUNT' ? 2 : 1\r\n          // 限时限量详情\r\n          // wx.navigateTo({\r\n          //   url: `/subPack_market/pages/seckill_index/time_limited/time_limited?type=${type}&channelType=${this.channelType}&actSn=${this.item.actSn}&qrCode=&timeId=${this.item.timeId || 0}&marketActId=${this.param.marketActId}`\r\n          // })\r\n        } else {\r\n          let query = {\r\n            actSn: this.item.actSn,\r\n            channelType: this.channelType\r\n          }\r\n          linkPage.linkPage(this.item.actType, query)\r\n          // let data = this.item\r\n          // if (data.actType == 'ACT_SPECIAL_OFFER') {\r\n          // } else if (data.actType == 'ACT_GIFT_PACKAGE') {\r\n          // } else if (data.actType == 'ACT_ASSEMBLE') {\r\n          // }\r\n        }\r\n      }\r\n    },\r\n    created() {\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n  .act-item {\r\n    width: 342px;\r\n    margin-bottom: 18px;\r\n    background: #FFFFFF;\r\n    border: 1px solid #F5F3F0;\r\n    border-radius: 12px;\r\n    overflow: hidden;\r\n    box-sizing: border-box;\r\n    display: inline-block;\r\n    min-height: 401px;\r\n  }\r\n  .act-item-two {\r\n    width: 316px !important;\r\n    background: #FFFFFF;\r\n    margin-right: 18px;\r\n    border: 1px solid #F5F3F0;\r\n    border-radius: 12px;\r\n    overflow: hidden;\r\n    min-height: 384px;\r\n    box-sizing: border-box;\r\n    display: inline-block;\r\n\r\n  }\r\n  .act-item-two .logo-bar {\r\n    overflow: hidden;\r\n    height: 194px;\r\n    position: relative;\r\n    border-radius: 12px 12px 0 0;\r\n  }\r\n\r\n  .act-logo{\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n\r\n  .act-item .logo-bar{\r\n    overflow: hidden;\r\n    height: 210px;\r\n    position: relative;\r\n    border-radius: 12px 12px 0 0;\r\n  }\r\n  .sale-out{\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background: rgba(60,63,77,0.7);\r\n  }\r\n  .sale-out .sale-over{\r\n    width: 120px;\r\n    height: 106px;\r\n    position: relative;\r\n  }\r\n\r\n  .act-tag{\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    height: 40px;\r\n    line-height: 40px;\r\n    font-size: 24px;\r\n    color: #FFFFFF;\r\n    width: 144px;\r\n  }\r\n  .act-tag text{\r\n    width: 100%;\r\n    height: 100%;\r\n    position: absolute;\r\n    left: 0;\r\n    top:0 ;\r\n  }\r\n  .act-tag-bg{\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n  .distance {\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 85px;\r\n    bottom: 0;\r\n    right: 0;\r\n    font-size: 24px;\r\n    color:#FFDDA4;\r\n    text-align: right;\r\n    padding-top: 40px;\r\n    padding-right: 12px;\r\n    background-image: linear-gradient(180deg, rgba(0,0,0,0.00) 0%, rgba(36,39,53,0.45) 99%);\r\n    text-shadow: 0 2px 4px rgba(0,0,0,0.50);\r\n  }\r\n  .icon-distance {\r\n    width: 19px;\r\n    height: 22px;\r\n    margin-right: 5px\r\n  }\r\n  .act-info{\r\n    padding: 18px 16px 24px\r\n  }\r\n  .shop-name .act-rec{\r\n    flex-shrink: 0;\r\n    width: 60px;\r\n    height: 34px;\r\n    line-height: 34px;\r\n    font-size: 24px;\r\n    color: #FFFFFF;\r\n    background-image: linear-gradient(90deg, #FF8800 0%, #FF6300 100%);\r\n    border-radius: 6px;\r\n    margin-right: 10px\r\n  }\r\n  .shop-name .name{\r\n    font-size: 24px;\r\n    color: #666564;\r\n  }\r\n  .shop-name image {\r\n    margin-right: 10px;\r\n    width: 30px;\r\n    height: 30px;\r\n  }\r\n  .shop-name .travel {\r\n    margin-top: -2px;\r\n    margin-bottom: -2.5px;\r\n  }\r\n  .card-name{\r\n    font-size: 26px;\r\n    line-height: 28px;\r\n    color: #11131A;\r\n    margin-top: 15px;\r\n  }\r\n  .group-name2 {\r\n    font-size: 26px;\r\n    color: #1A1919;\r\n    line-height: 36px;\r\n    height: 72px;\r\n    margin-top: -6px;\r\n  }\r\n  .gift-name {\r\n    font-size: 26px;\r\n    color: #11131A;\r\n    line-height: 32px;\r\n    height: 66px;\r\n    overflow: hidden;\r\n    text-overflow: ellipsis;\r\n    display: -webkit-box;\r\n    -webkit-line-clamp: 2;\r\n    line-clamp: 2;\r\n    -webkit-box-orient: vertical;\r\n    white-space: normal;\r\n  }\r\n\r\n  .apply-shop {\r\n    position: absolute;\r\n    bottom: 12px;\r\n    right: 12px;\r\n    padding: 0 10px;\r\n    height: 36px;\r\n    line-height: 36px;\r\n    font-size: 20px;\r\n    background: rgba(26, 25, 25, .5);\r\n    border-radius: 18px 18px 0 18px;\r\n    color: rgb(255, 255, 255, 0.8);\r\n  }\r\n\r\n  .remain-num {\r\n    position: absolute;\r\n    bottom: 12px;\r\n    right: 12px;\r\n    padding: 0 10px;\r\n    height: 36px;\r\n    line-height: 36px;\r\n    font-size: 20px;\r\n    background-image: linear-gradient(90deg, #F7420D 0%, #F50C05 100%);\r\n    border-radius: 18px 18px 0 18px;\r\n    color: #ffffff;\r\n  }\r\n  .gift-vip-text {\r\n    left: 0;\r\n    top: 0;\r\n    padding: 0 10px;\r\n    font-size: 24px;\r\n    color: #8C4B00;\r\n    line-height: 42px;\r\n    background-image: linear-gradient(270deg, #FFDCA3 0%, #FFC694 100%);\r\n    border-radius: 12px 0 18px 0;\r\n    box-sizing: border-box;\r\n  }\r\n  .act-footer {\r\n    margin-top: 11px;\r\n  }\r\n  .price {\r\n    margin-top: 13px;\r\n  }\r\n\r\n  span,div {\r\n    line-height: 1;\r\n  }\r\n  .group-label {\r\n    background: #FDF4E8;\r\n    border: 1px solid #FFCE78;\r\n    border-radius: 4px;\r\n    font-size: 20px;\r\n    color: #F29600;\r\n    height: 28px;\r\n    line-height: 28px;\r\n    padding: 0 8px;\r\n    box-sizing: border-box;\r\n  }\r\n  .group-icon {\r\n    width: 30px;\r\n    height: 30px;\r\n    margin-right: 6px;\r\n  }\r\n  .group-name {\r\n    font-size: 24px;\r\n    color: #E35323;\r\n    margin-top: -6px;\r\n  }\r\n  // .distributor-price {\r\n  //   width: auto;\r\n  //   height: 28px;\r\n  //   background: url('http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.5.6/label_bg.png');\r\n  //   background-size: 100% 100%;\r\n  //   border-radius: 3px;\r\n  // }\r\n  // .distributor-price image{\r\n  //   width: 36px;\r\n  //   height: 28px;\r\n  // }\r\n  // .distributor-price text {\r\n  //   padding: 0 8px 0 4px;\r\n  //   box-sizing: border-box;\r\n  //   color: #401E07;\r\n  // }\r\n</style>"]}]}