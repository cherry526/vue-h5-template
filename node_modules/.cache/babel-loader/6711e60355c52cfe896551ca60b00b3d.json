,CAACK,OAAJ,CAAY,GAAZ,MAAqB,CAAC,CAA1B,EAA6B;AAC3B,QAAIuB,UAAU,GAAG5B,GAAG,CAACK,OAAJ,CAAY,GAAZ,IAAmB,CAApC;AACA,QAAIP,GAAJ;;AACA,QAAGE,GAAG,CAACK,OAAJ,CAAY,GAAZ,MAAqB,CAAC,CAAzB,EAA4B;AAC1B,UAAIwB,QAAQ,GAAG7B,GAAG,CAACK,OAAJ,CAAY,GAAZ,CAAf;AACAP,MAAAA,GAAG,GAAGE,GAAG,CAACgB,SAAJ,CAAcY,UAAd,EAA0BC,QAA1B,CAAN;AACD,KAHD,MAGO;AACL/B,MAAAA,GAAG,GAAGE,GAAG,CAACgB,SAAJ,CAAcY,UAAd,CAAN;AACD;;AACD,QAAIE,IAAI,GAAGhC,GAAG,CAACU,KAAJ,CAAU,GAAV,CAAX;;AACA,SAAK,IAAIS,CAAC,GAAG,CAAb,EAAgBA,CAAC,GAAGa,IAAI,CAACxB,MAAzB,EAAiCW,CAAC,EAAlC,EAAsC;AACpCvB,MAAAA,GAAG,CAACoC,IAAI,CAACb,CAAD,CAAJ,CAAQT,KAAR,CAAc,GAAd,EAAmB,CAAnB,CAAD,CAAH,GAA6BsB,IAAI,CAACb,CAAD,CAAJ,CAAQT,KAAR,CAAc,GAAd,EAAmB,CAAnB,CAA7B;AACD;;AACD,WAAOd,GAAP;AACD;AACF,CAlBD,C,CAoBA;;;AACA,IAAMqC,eAAe,GAAG,SAAlBA,eAAkB,GAAwB;AAAA,MAAvBC,QAAuB,uEAAZ,OAAY;AAC9C,MAAIhC,GAAG,GAAGC,MAAM,CAACC,QAAP,CAAgBoB,IAA1B,CAD8C,CACX;;AACnC,MAAIW,QAAQ,GAAGhC,MAAM,CAACC,QAAP,CAAgBa,MAAhB,CAAuBmB,MAAvB,CAA8B,CAA9B,CAAf,CAF8C,CAEK;;AACnD,MAAIC,SAAS,GAAGnC,GAAG,CAACkC,MAAJ,CAAW,CAAX,EAAclC,GAAG,CAACK,OAAJ,CAAY,GAAZ,CAAd,CAAhB,CAH8C,CAGK;;AACnD,MAAI+B,OAAO,GAAG,EAAd;AAEA,MAAIC,GAAG,GAAG,IAAIC,KAAJ,EAAV;;AACA,MAAIL,QAAQ,IAAI,EAAhB,EAAoB;AAClB,QAAIM,WAAW,GAAGN,QAAQ,CAACzB,KAAT,CAAe,GAAf,CAAlB,CADkB,CACqB;;AACvC,SAAK,IAAIS,CAAC,GAAG,CAAb,EAAgBA,CAAC,GAAGsB,WAAW,CAACjC,MAAhC,EAAwCW,CAAC,EAAzC,EAA6C;AAC3C,UAAIuB,QAAQ,GAAGD,WAAW,CAACtB,CAAD,CAAX,CAAeT,KAAf,CAAqB,GAArB,CAAf,CAD2C,CACD;AAC1C;;AACA,UAAIgC,QAAQ,CAAC,CAAD,CAAR,IAAeR,QAAnB,EAA6B;AACzBK,QAAAA,GAAG,CAACI,IAAJ,CAASF,WAAW,CAACtB,CAAD,CAApB;AACH;AACF;AACF;;AACD,MAAIoB,GAAG,CAAC/B,MAAJ,GAAa,CAAjB,EAAoB;AAChB8B,IAAAA,OAAO,GAAG,MAAMC,GAAG,CAACK,IAAJ,CAAS,GAAT,CAAhB;AACH;;AACD1C,EAAAA,GAAG,GAAGmC,SAAS,GAAGC,OAAlB;AACA,SAAOpC,GAAP;AACD,CAtBD;;AAwBA,IAAM2C,KAAK,GAAG;AACVnE,EAAAA,IAAI,EAAJA,IADU;AAEViB,EAAAA,QAAQ,EAARA,QAFU;AAGVtB,EAAAA,UAAU,EAAVA,UAHU;AAIViB,EAAAA,QAAQ,EAARA,QAJU;AAKVG,EAAAA,QAAQ,EAARA,QALU;AAMVC,EAAAA,QAAQ,EAARA,QANU;AAOVb,EAAAA,MAAM,EAANA,MAPU;AAQVI,EAAAA,WAAW,EAAXA,WARU;AASVE,EAAAA,OAAO,EAAPA,OATU;AAUVC,EAAAA,UAAU,EAAVA,UAVU;AAWVG,EAAAA,UAAU,EAAVA,UAXU;AAYVQ,EAAAA,UAAU,EAAVA,UAZU;AAaVE,EAAAA,QAAQ,EAARA,QAbU;AAcVc,EAAAA,OAAO,EAAPA,OAdU;AAeVQ,EAAAA,OAAO,EAAPA,OAfU;AAgBVM,EAAAA,eAAe,EAAfA,eAhBU;AAiBVI,EAAAA,eAAe,EAAfA;AAjBU,CAAd;AAoBA,eAAeY,KAAf","sourcesContent":["\r\nimport Moment from 'moment'\r\n\r\n//处理时间\r\nconst formatTime = (date, type = 'YYYY-MM-DD HH:mm:ss') => {\r\n  if (date) {\r\n    date = typeof date === 'object' ? date : new Date(date)\r\n  } else {\r\n    date = new Date()\r\n  }\r\n  return Moment(date).format(type)\r\n}\r\n\r\n// 处理空格\r\nconst trim = s => {\r\n  return s.replace(/(^\\s*)|(\\s*$)/g, \"\")\r\n}\r\n\r\n// 验证姓名（中英文数字）限制15个字 可输入 中 英 文 数字 字母 \r\nconst isName = name => {\r\n  if(name) { \r\n    let reg= /^[\\u4e00-\\u9fa5A-Za-z0-9]{1,15}$/\r\n    if(name === 'null' || name === 'undefined'){\r\n      return false\r\n    }\r\n    return !!reg.test(name)\r\n  }\r\n}\r\n\r\n// 验证手机号码13-19的11位手机号码\r\nconst isCellphone = phone => {\r\n  if(phone) { \r\n    let reg= /^1[3456789][0-9]{9}$/\r\n    return !!reg.test(phone)\r\n  }\r\n}\r\n\r\n// 验证电话号码\r\nconst isPhone = phone => {\r\n  if(phone) { \r\n    let reg= /^[0-9\\-]+$/\r\n    return !!reg.test(phone)\r\n  }\r\n}\r\n\r\n// 验证输入的验证码（6位的正数字）\r\nconst isAuthcode = num => {\r\n  if(num) { \r\n    let reg= /^\\d{6}$/\r\n    return !!reg.test(num)\r\n  }\r\n}\r\n\r\n// 验证正整数\r\nconst isNumber = num => {\r\n  let reg= /^[0-9]\\d*$/\r\n  return !!reg.test(num)\r\n}\r\n\r\n// 验证用户密码（必须字母数字组合，6-16个字符）\r\nconst isPassword = pass => {\r\n  if(pass) { \r\n    let reg = /^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{6,16}$/\r\n    return !!reg.test(pass)\r\n  }\r\n}\r\n\r\n// 一位小数的正实数\r\nconst isApoint = num => {\r\n  if(num) { \r\n    let reg= /^[0-9]+(.[0-9]{1})?$/\r\n    return !!reg.test(num)\r\n  }\r\n}\r\n\r\n// 正数或一位小数\r\nconst decimals = num => {\r\n  if(num) { \r\n    let reg= /^[0-9]+([.][0-9]{1}){0,1}$/\r\n    return !!reg.test(num)\r\n  }\r\n}\r\n\r\n//验证一个对象是否有值为空\r\nconst emptyObj = obj => {\r\n  let res = true       \r\n   for(var key in obj){\r\n     if(!obj[key]){\r\n      res = false\r\n     }\r\n   }\r\n   return res\r\n}\r\n\r\n\r\n// 禁止为空、undefined和null\r\nconst textNormal = str => {\r\n  if (!str || str == 'undefined' || str == 'null') {\r\n    return false\r\n  }else{\r\n    return true\r\n  }\r\n}\r\n\r\n//截取路由参数 参数格式在#之后\r\nconst getDatas = (url = window.location) => {\r\n  var args = {}\r\n  url = url.slice(url.indexOf('?') + 1)\r\n  if (url.length > 0) {\r\n    var nameValueArr = url.split(\"&\") // 多参数\r\n    nameValueArr.forEach(v => {\r\n      let temp = v.split('=')\r\n      let key = temp[0]\r\n      let val = temp[1]\r\n      args[key] = val\r\n    })\r\n    return args\r\n  }\r\n}\r\n\r\n//获取路由参数 参数格式在# 之前\r\nconst getData = (url = window.location) => {\r\n  var args = {}\r\n  var argsStr = url.search; //获取URL参数字符串\r\n  if (argsStr.length > 0) {\r\n    argsStr = argsStr.substring(1);\r\n    var nameValueArr = argsStr.split(\"&\"); //多参数\r\n    for (var i = 0; i < nameValueArr.length; i++) {\r\n      var pos = nameValueArr[i].indexOf(\"=\");\r\n      if (pos == -1) continue; //如果没有找到就跳过\r\n      var argName = nameValueArr[i].substring(0, pos); //提取name\r\n      var argVal = nameValueArr[i].substring(pos + 1); //提取value\r\n      args[argName] = argVal\r\n    }\r\n    return args\r\n  }\r\n}\r\n\r\n// 去除链接上的token\r\nconst delData = () => {\r\n  if (window.location.href.indexOf('token') > -1 && window.location.href.indexOf('?') > -1 && window.location.href.indexOf('#') > -1) {\r\n    let tokenstr = window.location.href.match(/\\?(\\S*)#/)[1]\r\n    let lastarr = window.location.href.split('?' + tokenstr)\r\n    let lasturl = lastarr[0] + lastarr[1] + ''\r\n    return lasturl\r\n  } else {\r\n    return window.location.href\r\n  }\r\n}\r\n\r\n// 获取url中的参数 history模式下\r\nconst formatUrlParams = () => {\r\n  let url = window.location.href\r\n  let obj = {}\r\n  if (url.indexOf('?') !== -1) {\r\n    let startIndex = url.indexOf('?') + 1\r\n    let str;\r\n    if(url.indexOf('#') !== -1) {\r\n      let endIndex = url.indexOf('#')\r\n      str = url.substring(startIndex, endIndex)\r\n    } else {\r\n      str = url.substring(startIndex)\r\n    }\r\n    let strs = str.split('&')\r\n    for (let i = 0; i < strs.length; i++) {\r\n      obj[strs[i].split('=')[0]] = strs[i].split('=')[1]\r\n    }\r\n    return obj\r\n  }\r\n}\r\n\r\n// 去除url中指定参数\r\nconst deleteUrlParams = (paramKey = 'token') => {\r\n  var url = window.location.href;    //页面url\r\n  var urlParam = window.location.search.substr(1);   //页面参数\r\n  var beforeUrl = url.substr(0, url.indexOf(\"?\"));   //页面主地址（参数之前地址）\r\n  var nextUrl = \"\";\r\n\r\n  var arr = new Array();\r\n  if (urlParam != \"\") {\r\n    var urlParamArr = urlParam.split(\"&\"); //将参数按照&符分成数组\r\n    for (var i = 0; i < urlParamArr.length; i++) {\r\n      var paramArr = urlParamArr[i].split(\"=\"); //将参数键，值拆开\r\n      //如果键与要删除的不一致，则加入到参数中\r\n      if (paramArr[0] != paramKey) {\r\n          arr.push(urlParamArr[i]);\r\n      }\r\n    }\r\n  }\r\n  if (arr.length > 0) {\r\n      nextUrl = \"?\" + arr.join(\"&\");\r\n  }\r\n  url = beforeUrl + nextUrl;\r\n  return url\r\n}\r\n\r\nconst utils = {\r\n    trim,\r\n    emptyObj,\r\n    formatTime,\r\n    isNumber,\r\n    isApoint,\r\n    decimals,\r\n    isName,\r\n    isCellphone,\r\n    isPhone,\r\n    isAuthcode,\r\n    isPassword,\r\n    textNormal,\r\n    getDatas,\r\n    getData,\r\n    delData,\r\n    formatUrlParams,\r\n    deleteUrlParams\r\n}\r\n\r\nexport default utils\r\n"]}]}