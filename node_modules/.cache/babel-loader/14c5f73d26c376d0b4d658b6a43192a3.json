'>\r\n        <button class=\"ft34 tac\" :class=\"btnDisabled ? 'cB5' : 'c99' \"  id=\"STATIS_BUY\">{{btnText}}</button>\r\n      </div>\r\n    </div>\r\n    <!-- 正常活动进入 -->\r\n    <template v-else>\r\n      <div class=\"footer pof\" v-if='!ready && showBtnTxt'>\r\n        <active-footer :info=\"info\" :btnText=\"btnText\" :btnDisabled=\"btnDisabled\" :selCashred=\"selCashred\" :selConsumer=\"selConsumer\" @success=\"handlerEvent\"></active-footer>\r\n      </div>\r\n    </template>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n  import activeBrand from '@components/active/activeBrand'\r\n  import activeNotice from '@components/active/activeNotice'\r\n  import activeGraphic from '@components/active/activeGraphic'\r\n  import activeRoom from '@components/active/activeRoom'\r\n  import activeHeader from '@components/active/activeHeader'\r\n  import activeCoupon from '@components/active/activeCoupon'\r\n  import activeCash from '@components/active/activeCash'\r\n  import activeFooter from '@components/active/activeFooter'\r\n\r\n  export default {\r\n    name: 'specialSale',\r\n    mixins: [],\r\n    computed: {},\r\n    components:{\r\n      activeHeader,\r\n      activeCoupon,\r\n      activeCash,\r\n      activeGraphic,\r\n      activeBrand,\r\n      activeRoom,\r\n      activeNotice,\r\n      activeFooter\r\n    },\r\n    data() {\r\n      return {\r\n        ready: false,\r\n        selCashred: {},\r\n        btnText: '立即抢购',\r\n        showBtnTxt: true,\r\n        fixedDetail:{},\r\n        imgUrls: [\r\n          // { \r\n          //   type: 'video',\r\n          //   video: 'https://toss.wbgj.cn/vip_legal_right/15942901170001594290117728_673635.mp4',\r\n          //   urlPath: 'https://toss.wbgj.cn/vip_legal_right/15943601460001594360145503_595808.jpeg'\r\n          // },\r\n          // { type: 'img', urlPath: 'https://toss.wbgj.cn/vip_legal_right/15943601460001594360145503_595808.jpeg'},\r\n          // { \r\n          //   type: 'video',\r\n          //   video: 'https://toss.wbgj.cn/vip_legal_right/15942901170001594290117728_673635.mp4',\r\n          //   urlPath: 'https://toss.wbgj.cn/vip_legal_right/15943601460001594360145503_595808.jpeg'\r\n          // },\r\n          'https://toss.wbgj.cn/vip_legal_right/15943601460001594360145503_595808.jpeg',\r\n          'https://toss.wbgj.cn/vip_legal_right/15943601460001594360145503_595808.jpeg',\r\n        ],\r\n        remainTime: 3999000,\r\n        selConsumer: {},\r\n        channelType: '',\r\n        btnDisabled: false,\r\n        options: this.$route.query || {},\r\n        suitShops : [\r\n          {\r\n            address: \"南山区高新园\",\r\n            brandName: null,\r\n            cancelOfPayment: true,\r\n            distance: 5349.90904675849,\r\n            existsAct: true,\r\n            existsBaseDiscount: false,\r\n            lat: 22.53332,\r\n            lng: 113.93041,\r\n            shopId: 47,\r\n            shopName: \"余李冰的店3\",\r\n            shopPic: \"https://toss.wbgj.cn/jkb_shop/1550298855000_767071.png\",\r\n            shopSn: \"shop_7l44IXU2qRd\",\r\n            tel: \"15218130026\"\r\n          }\r\n        ],\r\n        info: {\r\n          shopShow: true,\r\n          lables:'1, 2, 3',\r\n          ableUseWallet: true,\r\n          actBannerList: [],\r\n          actEndTime: 1682783999000,\r\n          actExplain: null,\r\n          actId: 4094,\r\n          actName: \"余-特卖-渠道\",\r\n          shopName: '哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈哈',\r\n          shopPic: \"https://toss.wbgj.cn/vip_legal_right/15874517480001587451747311_826202.jpeg\",\r\n          actRecommendReason: \"\",\r\n          actRule: null,\r\n          actStartTime: 1587398400000,\r\n          actState: 2,\r\n          actType: \"ACT_SPECIAL_OFFER\",\r\n          backgroundPic: null,\r\n          backgroundType: 0,\r\n          buyLimit: 1,\r\n          childEnumKey: \"ACT_SPECIAL_OFFER\",\r\n          countDownHour: 0,\r\n          limitActState: null,\r\n          limitCount: null,\r\n          limitEndTime: null,\r\n          limitFrequency: 0,\r\n          limitStartTime: null,\r\n          limitState: false,\r\n          mediaList: [],\r\n          noVipPrice: 0,\r\n          offBackBuy: 0,\r\n          originalPrice: 10000,\r\n          panicBuyingPrice: 0,\r\n          promotion: null,\r\n          refreshVipCardInfo: false,\r\n          refundAllow: 3,\r\n          remainTime: 87551535509,\r\n          residueCount: null,\r\n          restrictState: 1,\r\n          sellPrice: 0,\r\n          showTime: true,\r\n          startRemainTime: 0,\r\n          topPic: null,\r\n          type: null,\r\n          viptake: null,\r\n          wantCnt: 0,\r\n          // 卡券详情\r\n          appointmentTime: 1,\r\n          cardCash: 10000,\r\n          cardId: 801,\r\n          cardName: \"10000元代金券\",\r\n          cardNum: null,\r\n          cardPicList: [{cardPic: \"https://toss.wbgj.cn/jkb_plat/15646456720001564645671882_622377.jpeg\"}],\r\n          cardStyle: 1,\r\n          cardType: \"CARD_CASH_COUPON\",\r\n          cardWorth: 10000,\r\n          comboDetail: [],\r\n          costPirce: 0,\r\n          createType: 1,\r\n          describe: \"<p>爱迪生阿萨德阿萨德撒旦撒大大大叔的撒爱迪生大神阿萨德阿达是大萨达是爱迪生阿达撒打算的撒的萨达大大的撒旦阿萨德撒大神爱迪生爱迪生阿达&nbsp;大萨达是&nbsp;大神&nbsp;</p>\",\r\n          description: \"酒水、茶位费除外 全场正价商品可以使用，促销、特价商品不参与\",\r\n          detailId: 1410,\r\n          endTime: 1598889599000,\r\n          houseUrl: '我是复制的内容呀',\r\n          importType: 1,\r\n          itemDetail: null,\r\n          lables: \"cm\",\r\n          limitFunds: 0,\r\n          limitStackable: 0,\r\n          limitTime: 0,\r\n          limitTimeInterval: \"营业时间可用\",\r\n          limitTimeIntervalDate: \"营业时间可用\",\r\n          overlayUse: false,\r\n          productShap: 1,\r\n          rechargeType: 0,\r\n          refreshVipCardInfo: false,\r\n          reserveDays: 0,\r\n          reserveRule: 0,\r\n          rightPackSn: \"package_7930334333532897280\",\r\n          shopName: \"叮咚叮咚叮咚。。。。。。。。\",\r\n          shopPic: \"https://toss.wbgj.cn/jkb_plat/15662650630001566265060075_722846.jpeg\",\r\n          shopShow: true,\r\n          shopSn: \"shop_jRWNs4n7jRW\",\r\n          showStyle: \"\",\r\n          stackable: false,\r\n          startTime: 1561910400000,\r\n          startValidity: 0,\r\n          storageDetail: {},\r\n          subscribeState: 0,\r\n          supplierCode: \"\",\r\n          timeType: 2,\r\n          useCondition: 1,\r\n          usePeriod: false,\r\n          validityDays: 0,\r\n          validityType: 1,\r\n        }\r\n      }\r\n    },\r\n    methods: {\r\n      handlerEvent() {\r\n        if (this.btnDisabled) return // 按钮置灰时，无法点击购买按钮\r\n        let info = this.info\r\n        if (info.supplierCode == 'PARTY_ZWY' && [3, 4, 5].includes(info.validityType)) {\r\n          // 自我游特卖时 && 需要选定日期   弹出选择日历\r\n\r\n          if (!this.calendarMask) {\r\n            this.setData({\r\n              calendarMask: true\r\n            })\r\n            return\r\n          }\r\n          if (!this.selDate.startTime) { // 未选择时间\r\n            wx.showToast({\r\n              title: '请选择游玩日期',\r\n              icon: 'none'\r\n            })\r\n            this.setData({\r\n              calendarMask: true\r\n            })\r\n          } else {\r\n            if (info.validityType == 4 || info.validityType == 5) { // 游客选定日期延后X有效 || 游客选定日期指定时间有效\r\n              wx.showModal({\r\n                title: '确定出游时间?',\r\n                content: `${util.timestampToTime(this.selDate.startTime, '/')} ~ ${util.timestampToTime(this.selDate.endTime, '/')}`,\r\n                success: (res) => {\r\n                  if (res.confirm) {\r\n                    this.placeOrder()\r\n                  } else if (res.cancel) {\r\n                    console.log('用户点击取消')\r\n                  }\r\n                }\r\n              })\r\n\r\n            } else {\r\n              wx.showModal({\r\n                title: '确定出游时间?',\r\n                content: `${util.timestampToTime(this.selDate.startTime, '/')}`,\r\n                success: (res) => {\r\n                  if (res.confirm) {\r\n                    this.placeOrder()\r\n                  } else if (res.cancel) {\r\n                    console.log('用户点击取消')\r\n                  }\r\n                }\r\n              })\r\n\r\n            }\r\n          }\r\n          return\r\n        }\r\n        this.placeOrder()\r\n      },\r\n    },\r\n    created() {\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n  .footer1 {\r\n    width: 100%;\r\n    box-sizing: border-box;\r\n    height: 142px;\r\n    text-align: center;\r\n    bottom: 0;\r\n  }\r\n  .footer1 img {\r\n    position: relative;\r\n    z-index: -1;\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n  .footer1 .footer-text {\r\n    top: 0;\r\n    padding: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n  .footer1 form {\r\n    position: absolute;\r\n    top: 0;\r\n    height: 100%;\r\n    box-sizing: border-box;\r\n    width: 100%;\r\n  }\r\n  .footer1 button {\r\n    font-size: 34px;\r\n    color: #fff;\r\n    line-height: 127px;\r\n  }\r\n</style>"],"sourceRoot":"src/views/active"}]}