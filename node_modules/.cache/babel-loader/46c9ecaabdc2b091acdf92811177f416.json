,\r\n      // [ ['拼团', 'ACT_ASSEMBLE'], routerPage.jumpSpellGroup],\r\n\r\n    ])\r\n    pageMap.forEach((key, value) => {\r\n      // console.log(key, value, '---------------map')\r\n      if(value.includes(initName)) {\r\n        key(query)\r\n      }\r\n    })\r\n  }\r\n}\r\n\r\nexport default routerPage\r\n\r\n// const linkPage = (initName, query = {}, channelType = 'CHANNEL_ACTIVITY_LABEL_SHOW') => {\r\n//   // 跳转页面方法\r\n//   const jumpPage = (name, fn) => {\r\n//     fn && fn()\r\n//     router.push({ name, query })\r\n//   }\r\n//   // 首页\r\n//   const jumpIndex = () => {\r\n//     jumpPage('home')\r\n//   }\r\n//   // 大礼包\r\n//   const jumpGift = () => {\r\n//     jumpPage('giftBag')\r\n//   }\r\n//   // 特卖\r\n//   const jumpSpecial = () => {\r\n//     jumpPage('specialSale')\r\n//   }\r\n//   const routerMap = new Map([\r\n//     [ ['首页', 'APP_INDEX', 'PAGE_INDEX'], jumpIndex],\r\n//     [ ['大礼包', 'BANNER_OUTSIDE_BIG_GIFT_PAGE'], jumpGift],\r\n//     [ ['特卖', 'BANNER_SPECIAL_SELL_PAGE', 'BANNER_SELF_TRAVEL_SPECIAL_ACT'], jumpSpecial],\r\n//   ])\r\n//   routerMap.forEach((key, value) => {\r\n//     console.log(key, value, '---------------map')\r\n//     if(value.includes(initName)) {\r\n//       key()\r\n//     }\r\n//   })\r\n// }\r\n\r\n// export default linkPage\r\n\r\n"]}]}