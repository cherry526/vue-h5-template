<!--\r\n * @Description: \r\n * @Version: 1.0\r\n * @Autor: cherry\r\n * @Date: 2020-06-04 14:15:58\r\n * @LastEditors: cherry\r\n * @LastEditTime: 2020-08-06 10:30:59\r\n--> \r\n<template>\r\n  <div id=\"app\">\r\n    <!-- <transition :name=\"transitionName\">  -->\r\n      <keep-alive>\r\n        <router-view v-if=\"$route.meta.keepAlive\" class=\"child-view\" v-wechat-title=\"$route.meta.title\"> </router-view>  \r\n      </keep-alive>\r\n      <router-view class=\"child-view\" v-wechat-title=\"$route.meta.title\" v-if=\"!$route.meta.keepAlive\"> </router-view>\r\n    <!-- </transition> -->\r\n    <!-- 返回首页 -->\r\n    <back-icon></back-icon>\r\n  </div>\r\n</template>\r\n<script>\r\nimport getWXSign from \"@/utils/wx\"\r\nimport { mapGetters, mapActions, mapMutations } from \"vuex\";\r\nconst defaultSettings = require('@/config/index.js')\r\nconst { AUTH_API } = defaultSettings\r\nexport default {\r\n  name: \"App\",\r\n  data() {\r\n    return {\r\n      transitionName: 'transition-page'\r\n    }\r\n  },\r\n  //监听路由的路径，可以通过不同的路径去选择不同的切换效果  \r\n  watch: {  \r\n    '$route' (to, from) {  \r\n      if(to.path == '/'){  \r\n        this.transitionName = '';  \r\n      }else{  \r\n        this.transitionName = 'transition-page';  \r\n      }  \r\n    }  \r\n  },\r\n\r\n  methods: {\r\n    ...mapActions('index', [\r\n      // 获取当前位置经纬度信息\r\n      'setLocationCityInfo'\r\n    ]),\r\n    // // 初始化微信\r\n    // initWx() {\r\n    //   $eventBus.$on('replaceFlag',(res) => {\r\n    //     console.log(res,'code----------------------app')\r\n    //     if(res) {\r\n    //       getWXSign().then(wx => {\r\n    //         console.log(wx, '-----------wx')\r\n    //         window.$wx = wx\r\n    //       }).catch(res => {\r\n    //         console.log(res, '---------getWXSignError')\r\n    //       })\r\n    //     }\r\n    //   })\r\n    // },\r\n  },\r\n  created() {\r\n    console.log('app------created')\r\n  },\r\n  mounted() {\r\n    // this.initPage()\r\n    console.log('app------mounted', this.$route, '---------现在的路由')\r\n    if(!this.$save.get('session', 'token')) {\r\n      this.setLocationCityInfo()\r\n    }\r\n    // this.initWx()\r\n    \r\n  },\r\n  destroyed() {\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import './styles/index.scss';\r\n#app {\r\n  height: 100%;\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  min-height: 100vh;\r\n}\r\n.child-view {  \r\n  position: absolute;  \r\n  left: 0;  \r\n  top: 0;  \r\n  width: 100%;  \r\n  height: 100%;  \r\n  transition: all .5s cubic-bezier(.55,0,.1,1);  \r\n}  \r\n.transition-page-enter {  \r\n  opacity: 0;  \r\n  -webkit-transform: translate(30px, 0);  \r\n  transform: translate(30px, 0);  \r\n}  \r\n.transition-page-leave-active {  \r\n  opacity: 0;  \r\n  -webkit-transform: translate(-30px, 0);  \r\n  transform: translate(-30px, 0);  \r\n}  \r\n\r\n</style>\r\n"],"sourceRoot":"src"}]}