tate === 4) {\r\n      s = '活动已暂停'\r\n    } else if (state === 6) {\r\n      s = '活动已结束'\r\n    } else if (state === 7) {\r\n      s = '活动未开始'\r\n    }\r\n    return s\r\n  },\r\n  // 限时限量活动状态\r\n  limitedState: (state, time) => {\r\n    const stateObj = {\r\n      1: '马上抢',\r\n      2: '已结束',\r\n      3: '已抢完',\r\n      4: '已开抢',\r\n      5: '抢购中',\r\n      6: '即将开抢',\r\n      7: '明日开抢',\r\n      8: '提醒我',\r\n      9: '已设置'\r\n    }\r\n    return stateObj[state] || (Moment(date).format('MM.DD') + '开抢')\r\n  },\r\n  // 限时限量 时间状态\r\n  getDayName: (od) => {\r\n    var date = new Date(od)\r\n    var td = new Date();\r\n    td = new Date(td.getFullYear(), td.getMonth(), td.getDate())\r\n    od = new Date(od)\r\n    od = new Date(od.getFullYear(), od.getMonth(), od.getDate())\r\n    var xc = (od - td) / 1000 / 60 / 60 / 24;\r\n    let h = formatNumber(date.getHours())\r\n    let m = formatNumber(date.getMinutes())\r\n    if (xc < -2) {\r\n      return -xc + \"天前\";\r\n    } else if (xc < -1) {\r\n      return \"前天\";\r\n    } else if (xc < 0) {\r\n      return \"昨天\";\r\n    } else if (xc == 0) {\r\n      return \"今日\" + h + ':' + m + '开抢';\r\n    } else if (xc < 2) {\r\n      return \"明日\" + h + ':' + m + '开抢';\r\n    } else if (xc < 3) {\r\n      return \"后天开始\";\r\n    } else {\r\n      return xc + \"天后开抢\";\r\n    }\r\n  },\r\n  // banner获取第一张\r\n  bannerFirst: (val) => {\r\n    return val ? val.split(';')[0] : ''\r\n  },\r\n  // 卡券类型\r\n  couponType(val) {\r\n    var obj = {\r\n      'CARD_CASH_COUPON':'代金券',\r\n      'CARD_ITEM_COUPON':'单品券',\r\n      'CARD_MULTI_COUPON':'套餐券',\r\n      'CARD_STORAGE':'寄存卡'\r\n    }\r\n    return obj[val] || ''\r\n  }\r\n}\r\n"]}]}