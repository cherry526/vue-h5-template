量特惠' : '限时特惠' }}\r\n                <div v-if=\" activeType && info.timeState == 1\" class=\"skill_want\">{{info.wantCnt || 0}}人想买</div>\r\n              </div>\r\n              <div class=\"flex\">\r\n                <span class=\"cff ft32\">¥<span class=\"ft50 bold\">{{ info.worth/100 }}</span></span>\r\n                <div class=\"ft22 cf51 consumer-price flex\">\r\n                  券后<div class=\"bold ft26\">¥<span class=\"ft38\">{{ (info.worth - selConsumer.couponMoney) > 0 ? (((info.worth - selConsumer.couponMoney)/100) | formatFloat) : 0 }}</span></div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"skill_pirce\" v-else>\r\n              <!-- ios手机虚拟产品不显示价格 -->\r\n              <div class=\"flex-b-e\"> \r\n                <span class=\"ft32\">¥</span>\r\n                <span class=\"bold lh1\">{{info.sellPrice/100 || 0}}</span>\r\n                <img v-if=\"info.buyLimit == 1 && info.noVipPrice && (info.noVipPrice != info.sellPrice)\" src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/label_jg.png\" class=\"vip_logo\">\r\n              </div>\r\n              <div class=\"ft26 skill_cost flex\">\r\n                <div v-if=\" info.timeState == 1 \" class=\"skill_want\">{{info.wantCnt || 0}}人想买</div>\r\n                <span v-if=\"info.buyLimit == 1 && info.noVipPrice && (info.noVipPrice != info.sellPrice) \">非会员价¥{{info.noVipPrice/100 || 0}}</span>\r\n                <span class=\"mg10\" v-if=\"info.buyLimit == 1 && info.noVipPrice && (info.noVipPrice != info.sellPrice) \">|</span>\r\n                <span>原价¥{{ info.worth/100 || 0 }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"ft22 skill_head_right flex-co-s\">\r\n            <!-- 限量 -->\r\n            <template v-if=\"activeType\">\r\n              <div class=\"flex-co-e\">\r\n                <template v-if=\"info.timeState == 1\"> \r\n                  <div class=\"skill_line line_no flex-cc\">\r\n                    <span class=\"only_no\">限量{{info.limitCount || 0}}份</span>\r\n                  </div>\r\n                  <div class=\"cfc\" v-if=\"info.timeState == 1\">{{headline}}</div>\r\n                </template>\r\n                <template v-else>\r\n                  <div class=\"skill_line\" v-if=\"info.residueCount > 0\">\r\n                    <div class=\"skill_strip\" :style=\"{ width: info.residueCount/info.limitCount*100 + '%' }\"></div>\r\n                    <span class=\"skill_only\">仅剩{{info.residueCount}}份</span>\r\n                  </div>\r\n                  <div class=\"skill_line line_no flex-cc\" v-else>\r\n                    <span class=\"only_no\">仅剩{{info.residueCount}}份</span>\r\n                  </div>\r\n                  <div class=\"c2f1\">数量有限 售完为止</div>\r\n                </template>\r\n              </div>\r\n            </template>\r\n        \r\n            <!-- 限时 -->\r\n            <template v-else>\r\n              <div class=\"flex-co-e\" v-if=\"info.timeState == 1\">\r\n                <div class=\"cfc lh1 skill-time-name\">{{headline}}</div>\r\n                <div class=\"flex-cc\"><span class=\"date_num\">{{downTime.hour}}</span><span class=\"date_dot\">:</span><span class=\"date_num\">{{downTime.minute}}</span><span class=\"date_dot\">:</span><span class=\"date_num\">{{downTime.second}}</span></div>\r\n              </div>\r\n              <div class=\"flex-co-e\" v-else>\r\n                <div class=\"c2f1 lh1 skill-time-name\">距结束还剩</div>\r\n                <div class=\"flex-cc\"><span class=\"date_num\">{{downTime.hour}}</span><span class=\"date_dot\">:</span><span class=\"date_num\">{{downTime.minute}}</span><span class=\"date_dot\">:</span><span class=\"date_num\">{{downTime.second}}</span></div>\r\n              </div>\r\n            </template>\r\n          </div>\r\n        </div>\r\n      </template>\r\n\r\n      <!-- 分销金额 -->\r\n      <!-- <div class=\"distributor-price\" v-if=\" info.promotion > 0 && wxUser.distributorFlag \">\r\n        <span class=\"label ft28\">分享赚</span>\r\n        <span class=\"price ft26\">最高可获得收益{{ info.promotion/100 }}元</span>\r\n      </div> -->\r\n\r\n      <!-- 头部内容 -->\r\n      <div class=\"skill_top\">\r\n        <div class=\"flex-b w pr30\">\r\n          <div class=\"skill_name text-two bold\" >\r\n            <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/label_zx03.png\" class=\"vip_logo_label disb\">\r\n            <span class=\"skill_title\">{{info.actName}}</span>\r\n          </div>\r\n          <!-- <div class=\"skill_name text-two bold\" >\r\n            <div style=\"display: inline;\">\r\n              <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/label_zx03.png\" class=\"vip_logo_label\" v-if=\"info.buyLimit == 2\">\r\n            </div>\r\n            <span class=\"skill_title\">{{info.actName}} </span>\r\n          </div>\r\n          <slot name=\"share\"></slot> -->\r\n        </div>\r\n        <div class=\"flex-c-b w skill-label\">\r\n          <refund-label :info.sync=\"info\"></refund-label>\r\n          <div class=\"c9E ft20\"  v-if=\"info.timeState != 1\">\r\n            <span v-if=\"info.limitActState == 10\">每人限{{info.limitFrequency}}份 | </span>\r\n            <span>已售</span>\r\n            <span v-if=\"activeType\">{{info.limitCount-info.residueCount}}</span>\r\n            <span v-else>{{info.receiveNum}}</span>\r\n          </div>\r\n          <div v-else></div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 限量和秒杀的预约提醒 -->\r\n      <div class=\"limit_tips_line w flex-v\" v-if=\"info.timeState == 1\">\r\n        <img v-if=\"activeType\" class=\"skill_img\" src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/font_ms.png\" >\r\n        <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/font_qg.png\" class=\"limit_img\" v-else>\r\n        <!-- <span>怕错过活动，点击预约提醒</span> -->\r\n        <span>活动暂未开始</span>\r\n      </div>\r\n    </template>\r\n    <!-- 常规活动头部 -->\r\n    <div class='active-header-box' v-else>\r\n      <!-- 开启了倒计时 -->\r\n      <template v-if=\" !hasExchange && showTime && remainTime > 0 && info.actState == 2 && info.remainNum > 0\">\r\n        <template v-if=\"!info.iosShowPrice && isIos && info.actType == 'ACT_SPECIAL_OFFER'\"> </template>\r\n        <template v-else>\r\n          <div class=\"active-header-time flex-c-b\">\r\n            <div class=\"active-time-left\" >\r\n              <!-- 消费券渠道进入显示价格 -->\r\n              <div class=\"consumer-box\" v-if=\"info.channelType == 'CHANNEL_CONSUMER_COUPON' || info.channelType == 'CHANNEL_FORTUNE_COUPON' \">\r\n                <div class=\"ft26 cff active-xianshi\" >限时特惠</div>\r\n                <div class=\"flex\">\r\n                  <span class=\"cff ft32\">￥<span class=\"ft50 bold\">{{ info.worth/100 }}</span></span>\r\n                  <div class=\"ft22 cf51 consumer-price flex\">\r\n                    券后<div class=\"bold ft26\">￥<span class=\"ft38\">{{ (info.worth - selConsumer.couponMoney) > 0 ? (((info.worth - selConsumer.couponMoney)/100) | formatFloat) : 0 }}</span></div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <template v-else>\r\n                <div class=\"flex-b-e\"> \r\n                  <span class=\"ft32\">¥</span>\r\n                  <span v-if=\"info.supplierCode == 'PARTY_ZWY' \">{{ (info.travelPrice || 0)/100 }}</span>\r\n                  <span v-else class=\"bold ft50 lh1\">{{info.sellPrice/100 || 0}}</span>\r\n                  <span v-if=\"info.supplierCode == 'PARTY_ZWY' \" class=\"ft32\">起</span>\r\n                  <!-- 显示金额 -->\r\n                  <template v-if=\"info.channelTrack ? info.packageGiftType == 1 : info.restrictState == 2\">\r\n                    <div style=\"display: inline;\">\r\n                      <img v-if=\"info.buyLimit == 1 && info.noVipPrice\" src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/label_jg.png\" class=\"vip_logo\">\r\n                    </div>\r\n                    <template v-if=\"(info.channelTrack ? info.packageGiftType == 2 : info.restrictState == 1) && info.freeReceiveType\">\r\n                      <span class=\"active-label\">{{freeType[info.freeReceiveType]}}</span>\r\n                    </template>\r\n                  </template>\r\n                  <!-- 免费 -->\r\n                  <template v-if=\"(info.channelTrack ? info.packageGiftType == 2 : info.restrictState == 1) && info.freeReceiveType\">\r\n                    <span class=\"active-label\">{{freeType[info.freeReceiveType]}}</span>\r\n                  </template>\r\n                </div>\r\n                <div class=\"ft26 act_cost flex\">\r\n                  <span v-if=\"info.buyLimit == 1 && info.noVipPrice && (info.noVipPrice != info.sellPrice)\">非会员价¥{{info.noVipPrice/100 || 0}}{{ info.supplierCode == 'PARTY_ZWY' ? '起':'' }}</span>\r\n                  <span class=\"mg10\" v-if=\"info.buyLimit == 1 && info.noVipPrice && (info.noVipPrice != info.sellPrice)\">|</span>\r\n                  <span>{{info.cardType === 'CARD_DIRECT_RECHARGE' ? '官方价' : '原价'}}¥{{ info.worth/100 || 0 }}</span>\r\n                </div>\r\n              </template>\r\n            </div>\r\n            <div class=\"active-time-right flex-col-c ft24 flex-1\">\r\n              <div class=\"ft22 mb12\" >距抢购结束</div>\r\n              <div>{{remainTime | countDown2}}</div>\r\n            </div>\r\n          </div>\r\n        </template>\r\n\r\n        <!-- 分销金额 -->\r\n        <!-- <div class=\"distributor-price\" v-if=\" info.promotion > 0 && wxUser.distributorFlag\">\r\n          <span class=\"label ft28\">分享赚</span>\r\n          <span class=\"price ft26\">最高可获得收益{{ info.promotion/100 }}元</span>\r\n        </div> -->\r\n\r\n        <div class=\"flex-c-b w active-title-box\">\r\n          <div class=\"text-two active-title1 bold\">\r\n            <img src=\"ht