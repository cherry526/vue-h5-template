   <div class=\"content pl24\" :style=\"index >= 1 ? 'margin-top: 18rpx' : ''\" v-for=\"(item,index) in formList\" :key=\"index\">\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowName\">\r\n            <div class=\"label ft32 c1A\">用户名<span class=\"cD9\" v-if=\"item.isFullName\">*</span></div>\r\n            <input class=\"flex-1\" :name=\"'userName' + index\" :value=\"item.userName\" placeholder=\"请输入用户名\">\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowIdentity\">\r\n            <div class=\"label ft32 c1A\">身份证<span class=\"cD9\" v-if=\"item.isFullIdentity\">*</span></div>\r\n            <input class=\"flex-1\" :name=\"idCard + index\" type=\"idcard\" :value=\"item.idCard\" placeholder=\"请输入身份证\">\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowPhone\">\r\n            <div class=\"label ft32 c1A\">手机号码<span class=\"cD9\" v-if=\"item.isFullPhone\">*</span></div>\r\n            <input class=\"flex-1\" :name=\"'phone' + index\" :value=\"item.phone\" type=\"number\" placeholder=\"请输入手机号码\">\r\n            <button v-if=\"!wxUser.phone && index == 0\" class=\"auth-phone\">授权</button>\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowEmial\">\r\n            <div class=\"label ft32 c1A\">邮箱<span class=\"cD9\" v-if=\"item.isfullEmial\">*</span></div>\r\n            <input class=\"flex-1\" :name=\"'email' + index\" :value=\"item.email\" placeholder=\"请输入邮箱\">\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowAdress\">\r\n            <div class=\"label ft32 c1A\">地址<span class=\"cD9\" v-if=\"item.isFullAdress\">*</span></div>\r\n            <picker mode=\"region\" :data-index=\"index\" :value=\"cityList[index]\" class=\"flex-1\" bindchange=\"addressPicker\">\r\n              <div class=\"flex-c-b\">\r\n                <span class=\"ft32 c1A\" v-if=\"cityList[index] && cityList[index].length > 0\">{{ cityList[index] }}</span>\r\n                <span class=\"ft32\" style=\"color: #C7C5C3;\" v-else >请选择地址</span>\r\n                <span class=\"iconfont iconicon_more ft18\" style=\"color: #C7C5C3; margin-right: 30rpx;\"></span>\r\n              </div>\r\n            </picker>\r\n          </div>\r\n          <div class=\"input-item flex-v\" v-if=\"item.isShowAdress\">\r\n            <div class=\"label ft32 c1A\">详细地址<span class=\"cD9\" v-if=\"item.isFullAdress\">*</span></div>\r\n            <input :name=\"'addressDetail' + index\" :value=\"item.addressDetail\" placeholder=\"请输入详细地址\">\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n\r\n      <!-- 备注 -->\r\n      <div class=\"controller\" v-if=\"info.receiveData.isShowRemarks\">\r\n        <div class=\"content pl24 remark\">\r\n          <div class=\"input-item flex\">\r\n            <div class=\"label ft32 c1A\">备注<span class=\"cD9\" v-if=\"info.receiveData.isfullRemark\">*</span></div>\r\n            <textarea name=\"remark\" :value=\"formList[0].remark\" placeholder=\"填写特殊情况和需求\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 紧急联系人 -->\r\n      <div class=\"controller\" v-if=\"info.urgentContact\">\r\n        <div class=\"title ft24 c66\">紧急联系人</div>\r\n        <div class=\"content pl24\">\r\n          <div class=\"input-item flex-v\">\r\n            <div class=\"label ft32 c1A\">联系人<span class=\"cD9\">*</span></div>\r\n            <input class=\"flex-1\" name=\"emergencyContact\" :value=\"formList[0].emergencyContact\" placeholder=\"请输入紧急联系人姓名\">\r\n          </div>\r\n          <div class=\"input-item flex-v\">\r\n            <div class=\"label ft32 c1A\">手机号码<span class=\"cD9\">*</span></div>\r\n            <input class=\"flex-1\" name=\"emergencyPhone\" type=\"number\" :value=\"formList[0].emergencyPhone\" placeholder=\"请输入紧急联系人手机号码\">\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"btn\">\r\n        <img class=\"bg-img\" src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/travel_information_btn.png\">\r\n        <button class=\"bold\" type=\"submit\">确定</button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    name: 'travelInformation',\r\n    mixins: [],\r\n    computed: {},\r\n    components:{},\r\n    data() {\r\n      return {\r\n        info: {},\r\n        cityList: [[]],\r\n        formList: [{\r\n          userName: '',\r\n          idCard: '',\r\n          phone: '',\r\n          email: '',\r\n          addressDetail: '',\r\n          city: [],\r\n          remark: '',\r\n          emergencyContact: '',\r\n          emergencyPhone: ''\r\n        }],\r\n        calendarMask: false,\r\n        selDate: {\r\n          startTime: '',\r\n          endTime: ''\r\n        }, // 自我游选中的时间\r\n        showCalendar: false,\r\n        showCalendarPro: false,\r\n        isShowLsit: [{\r\n          isShow: 'isShowName',\r\n          isEach: 'isEachPeopleNameWrite',\r\n          isFull: 'isFullName',\r\n          value: 'userName',\r\n          message: '用户名不能为空'\r\n        }, {\r\n          isShow: 'isShowPhone',\r\n          isEach: 'isEachPeoplePhoneWriter',\r\n          isFull: 'isFullPhone',\r\n          value: 'phone',\r\n          message: '手机号码不能为空'\r\n        }, {\r\n          isShow: 'isShowIdentity',\r\n          isEach: 'isEachPeopleIdentityWriter',\r\n          isFull: 'isFullIdentity',\r\n          value: 'idCard',\r\n          message: '身份证不能为空'\r\n        }, {\r\n          isShow: 'isShowEmial',\r\n          isEach: 'isEachPeopleEmialWriter',\r\n          isFull: 'isfullEmial',\r\n          value: 'email',\r\n          message: '邮箱不能为空'\r\n        }, {\r\n          isShow: 'isShowAdress',\r\n          isEach: 'isEachPeopleAdressWriter',\r\n          isFull: 'isFullAdress',\r\n        },\r\n        {\r\n          isFull: 'isfullRemark',\r\n          isShow: 'isShowRemarks',\r\n          value: 'remark',\r\n          message: '备注不能为空'\r\n        }]\r\n      }\r\n    },\r\n    methods: {\r\n        // 设置选中时间\r\n      setTime () {\r\n        let selDate= this.data.selDate\r\n        let date = new Date(selDate.startTime)\r\n        let _year = date.getFullYear() + '';\r\n        let _month = this.setString(date.getMonth() + 1);\r\n        let _day = this.setString(date.getDate())\r\n        let dateObj = {\r\n          span: _year + _month,\r\n          month: _month,\r\n          year: _year,\r\n          day: _day\r\n        }\r\n        console.log(dateObj)\r\n        this.selectComponent(\"#calendar\").showCalendarData(dateObj)\r\n      },\r\n\r\n      travelMonthInfo ({ detail }) {\r\n        // 接收日历信息\r\n        if (detail.dataType == 'selDate' && this.data.calendarMask){\r\n          // 选择日历信息\r\n          let info = this.data.info\r\n          info.sellPrice = detail.sellPrice\r\n          info.noVipPrice = detail.noVipPrice\r\n          this.setData({\r\n            info,\r\n            selDate: detail\r\n          })\r\n        }\r\n        \r\n      },\r\n\r\n      setString (span) {\r\n        return span > 9 ? span : '0'+span\r\n      },\r\n\r\n      openCalendar() {\r\n        this.setData({\r\n          calendarMask: true\r\n        })\r\n        this.setTime()\r\n      },\r\n\r\n      getInfo () {\r\n        this.$api.receive_condition({\r\n          actSn: this.options.actSn\r\n        }).then(res => {\r\n          if (res.state) {\r\n            res.info.receiveData = JSON.parse(res.info.receiveData)\r\n            res.info.worth = this.options.worth\r\n            this.info = res.info\r\n            if (res.info.conditionAsk == 1) {\r\n              // 需要每个人填写要求\r\n              this.setFormInfo(this.options.num)\r\n            } else {\r\n              this.setFormInfo(1)\r\n            }\r\n            if ([3, 4, 5].includes(res.info.validityType) && res.info.supplierCode == 'PARTY_ZWY') {\r\n              // 是否选择日期\r\n              this.showCalendar = true\r\n            }\r\n          }\r\n        })\r\n      },\r\n\r\n      setFormInfo (num) {\r\n        let travelInformation = wx.getStorageSync('travelInformation') || {}\r\n        \r\n        let formList = travelInformation.backList || []\r\n        let receiveData = this.data.info.receiveData\r\n\r\n        // 是否显示\r\n        let isShowLsit = this.data.isShowLsit\r\n\r\n        let formListNew = []\r\n        let cityList = []\r\n        for (let i=0;i < num;i++) {\r\n          if (formList[i]) {\r\n            formListNew.push(formList[i])\r\n            cityList.push(formList[i].city)\r\n          } else {\r\n            let formListNewObj = {\r\n              remark: '',\r\n              emergencyContact: '',\r\n              emergencyPhone: ''\r\n            }\r\n            if (i == 0) { // 第一个不用校验每个客人都需要输入\r\n              isShowLsit.map(item => { // 判断是否需要填写\r\n                if (receiveData[item.isShow]) {\r\n                  formListNewObj[item.isShow] = true\r\n                  if (item.isShow == 'isShowPhone') {\r\n                    formListNewObj.phone = this.data.wxUser.phone || ''\r\n                  }\r\n                }\r\n                if (receiveData[item.isShow] && item.isShow === 'isShowAdress') {\r\n                  cityList.push([])\r\n                }\r\n                if (receiveData[item.isFull]) {\r\n                  formListNewObj[item.isFull] = true\r\n                }\r\n              })\r\n            } else {\r\n\r\n              isShowLsit.map(item => { // 判断需要每个用户填写\r\n                if (receiveData[item.isEach] && item.isEach === 'isEachPeopleAdressWriter') { // 判断地址\r\n                  cityList.push([])\r\n                } \r\n                if (receiveData[item.isShow] && receiveData[item.isEach]) {\r\n                  formListNewObj[item.isShow] = true\r\n                  if (receiveData[item.isFull]) {\r\n                    formListNewObj[item.isFull] = true\r\n                  }\r\n                }\r\n                \r\n              })\r\n            }\r\n            formListNew.push(formListNewObj)\r\n          }\r\n          \r\n        }\r\n        console.log(cityList, 'cityListcityListcityList', formListNew)\r\n        this.setData({\r\n          formList: formListNew,\r\n          cityList\r\n        })\r\n      },\r\n\r\n      formSubmit({ detail }) {\r\n        if (this.data.calendarMask) {\r\n          this.setData({\r\n            calendarMask: false\r\n          })\r\n          return\r\n        } \r\n        let value = detail.value\r\n        let formList = this.data.formList\r\n        let cityList = this.data.cityList\r\n        let valueKey = ['addressDetail', 'email', 'idCard', 'phone', 'userName']\r\n        for (let i=0; i<formList.length; i++) {\r\n          for (let y = 0; y < valueKey.length;y++) {\r\n            let formKey = valueKey[y]+i\r\n            formList[i][valueKey[y]] = detail.value[formKey] || ''\r\n          }\r\n          formList[i].city = cityList[i] || []\r\n          formList[i].remark = value.remark || ''\r\n          formList[i].emergencyContact = value.emergencyContact || ''\r\n          formList[i].emergencyPhone = value.emergencyPhone || ''\r\n        } \r\n        this.rules(formList) // 过滤必填项\r\n      },\r\n\r\n      rules(formList) {\r\n        let info = this.data.info\r\n        let isRules = true\r\n        let rulesList = this.data.isShowLsit\r\n\r\n        for (let i = 0; i < formList.length; i++) {\r\n          rulesList.map(item => {\r\n            if (formList[i][item.isFull] && formList[i][item.isShow]) { // 判断必填项\r\n              if (item.isFull === 'isFullAdress') { // 判断地址\r\n                if (formList[i].city.length == 0) {\r\n                  wx.showToast({\r\n                    title: '请选择地址',\r\n                    icon: 'none'\r\n                  })\r\n                  isRules = false\r\n                  return false\r\n                }\r\n                if (formList[i].addressDetail == '') {\r\n                  wx.showToast({\r\n                    title: '请输入详细地址',\r\n                    icon: 'none'\r\n                  })\r\n                  isRules = false\r\n                  return false\r\n                }\r\n              }  else if (formList[i][item.value] == '' ) {\r\n                wx.showToast({\r\n                  title: item.message,\r\n                  icon: 'none'\r\n                })\r\n                isRules = false\r\n                return false\r\n              }\r\n            }\r\n            if (item.isFull === 'isFullPhone' && formList[i].phone) {\r\n              if (!verifyPhone(formList[i].phone)) {\r\n                wx.showToast({\r\n                  title: '手机号格式不正确',\r\n                  icon: 'none'\r\n                })\r\n                isRules = false\r\n                return false\r\n              }\r\n            } else if (item.isFull === 'isfullEmial' && formList[i].email) {\r\n              if (!verifyEmail(formList[i].email)) {\r\n                wx.showToast({\r\n                  title: '邮箱格式不正确',\r\n                  icon: 'none'\r\n                })\r\n                isRules = false\r\n                return false\r\n              }\r\n            } else if (item.isFull === 'isFullIdentity' && formList[i].idCard != '') {\r\n              if (formList[i].idCard.toString().length != 18) {\r\n                wx.showToast({\r\n                  title: '身份证格式不正确',\r\n                  icon: 'none'\r\n                })\r\n                isRules = false\r\n                return false\r\n              } \r\n            }\r\n          })\r\n\r\n\r\n\r\n          if (formList[i].userName) {\r\n            if (formList[i].userName.length > 10) {\r\n              wx.showToast({\r\n                title: '用户名长度不能超过10位',\r\n                icon: 'none'\r\n              })\r\n              isRules = false\r\n              return false\r\n            }\r\n          }\r\n\r\n          if (info.urgentContact && (formList[i].emergencyContact == '' || formList[i].emergencyPhone == '')) {\r\n            wx.showToast({\r\n              title: '请输入紧急联系人信息',\r\n              icon: 'none'\r\n            })\r\n            isRules = false\r\n            return false\r\n          }\r\n\r\n          if (formList[i].emergencyPhone != '') {\r\n            if (!verifyPhone(formList[i].emergencyPhone)) {\r\n              wx.showToast({\r\n                title: '紧急联系人手机号格式不正确',\r\n                icon: 'none'\r\n              })\r\n              isRules = false\r\n              return false\r\n            }\r\n          }\r\n\r\n          if (info.receiveData.isShowRemarks && info.receiveData.isfullRemark && formList[i].remark == '') {\r\n            wx.showToast({\r\n              title: '请填写备注信息',\r\n              icon: 'none'\r\n            })\r\n            isRules = false\r\n            return false\r\n          }\r\n\r\n          formList[i].address = formList[i].city.join(' ') + formList[i].addressDetail\r\n        }\r\n\r\n        if (this.data.showCalendar && !this.data.selDate.startTime) {\r\n          // 需要选择日期时\r\n          isRules = false\r\n          wx.showToast({\r\n            title: '请选择日期',\r\n            icon: 'none'\r\n          })\r\n          this.setData({\r\n            calendarMask: true\r\n          })\r\n        }\r\n\r\n        if (isRules) {\r\n          this.setInfo()\r\n        }\r\n      },\r\n\r\n      setInfo () {\r\n        let formList = this.data.formList\r\n        let formListNew = [], isNull = true\r\n        let valueKey = ['address', 'email', 'idCard', 'phone', 'userName', 'emergencyContact', 'emergencyPhone', 'remark']\r\n        for (let i=0;i<formList.length; i++) {\r\n          let obj  = {}, isShow = false\r\n          for (let y = 0; y < valueKey.length;y++) {\r\n            if (formList[i][valueKey[y]]) {\r\n              isNull = false\r\n              isShow = true\r\n              obj[valueKey[y]] = formList[i][valueKey[y]]\r\n            }\r\n          }\r\n          if (isShow) {\r\n            formListNew.push(obj)\r\n          } \r\n        }\r\n        formListNew.length == 0 ? formListNew.push({}) : \"\"\r\n        let travelInformation = {\r\n          backList: this.data.formList, // 用于回显数据\r\n          formList: formListNew, // 用于提交请求\r\n          selDate: this.data.selDate,\r\n          isNull\r\n        }\r\n        console.log(travelInformation, 'travelInformation')\r\n        // 校验成功保存数据\r\n        wx.setStorageSync('travelInformation', travelInformation)\r\n        wx.navigateBack({\r\n          delta: 1\r\n        })\r\n      },\r\n\r\n      addressPicker(event) {\r\n        let index = event.currentTarget.dataset.index\r\n        let value = event.detail.value\r\n        let cityList = this.data.cityList\r\n        cityList[index] = value\r\n        this.setData({\r\n          cityList\r\n        })\r\n      },\r\n    },\r\n    created() {\r\n      this.options.num = this.options.num || 1\r\n      let wxUser = app.globalData.wxUser\r\n      this.setData({\r\n        wxUser: wxUser,\r\n        channelType: options.channelType\r\n      })\r\n      this.getInfo()\r\n      if (options.selDate) {\r\n        console.log(options.selDate, JSON.parse(options.selDate))\r\n        this.setData({\r\n          selDate: JSON.parse(options.selDate),\r\n          actSn: options.actSn\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n</style>"],"sourceRoot":"src/views/active"}]}