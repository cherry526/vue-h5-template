AAKjB,OAAL,CAAaiB,WAAb,IAA4B,CALf;AAM1BH,QAAAA,IAAI,EAAJA;AAN0B,OAA5B,EAOGxB,IAPH,CAOQ,UAAAC,GAAG,EAAI;AACb,YAAIA,GAAG,CAACC,KAAJ,IAAaD,GAAG,CAACG,IAArB,EAA2B;AACzB,UAAA,MAAI,CAACwB,YAAL,GAAoB3B,GAAG,CAACG,IAAxB,EACA,MAAI,CAACjB,OAAL,GAAec,GAAG,CAACG,IAAJ,CAASyB,MAAT,GAAkB,IAAlB,GAAyB,KADxC;AAED;AACF,OAZD;AAaD,KA3EM;AA4EP;AACAC,IAAAA,OA7EO,mBA6EC7B,GA7ED,EA6EM;AAAA;;AACX,WAAK8B,MAAL,CAAY9B,GAAG,CAAC+B,OAAhB;AACAC,MAAAA,OAAO,CAACC,GAAR,CAAY,KAAKC,OAAjB;AACAC,MAAAA,UAAU,CAAC,YAAM;AACf,YAAG,MAAI,CAACC,MAAL,CAAYC,KAAZ,CAAkBC,OAArB,EAA8B;AAC5B,UAAA,MAAI,CAACJ,OAAL,CAAaI,OAAb,CAAqB;AACnBC,YAAAA,IAAI,EAAE;AADa,WAArB;AAGD,SAJD,MAIO;AACL,UAAA,MAAI,CAACL,OAAL,CAAaM,EAAb,CAAgB,CAAC,CAAjB;AACD;AACF,OARS,EAQP,GARO,CAAV;AASD;AAzFM;AAVW,CAAtB;AAsGA,eAAexD,aAAf","sourcesContent":["/*\r\n * @Description: \r\n * @Version: 1.0\r\n * @Autor: cherry\r\n * @Date: 2020-07-29 11:45:58\r\n * @LastEditors: cherry\r\n * @LastEditTime: 2020-08-04 20:03:05\r\n */ \r\n// import {\r\n//   mapGetters,\r\n//   mapActions,\r\n//   mapMutations\r\n// } from \"vuex\";\r\nimport utils from '@/filters/active'\r\nimport linkPage from '@/router/linkPage'\r\n// 首页相关\r\nconst entranceMixin = {\r\n  data () {\r\n    return {\r\n      endShow: false,\r\n      actList: []\r\n    }\r\n  },\r\n  // computed: {\r\n  //   ...mapGetters(['locationInfo'])\r\n  // },\r\n  methods: {\r\n    //特卖的适用门店列表\r\n    getSuitShops() {\r\n      this.$api.suitShop({\r\n        cardId: this.cardId,\r\n        detailId: this.detailId,\r\n        lat: this.locationInfo.latitude,\r\n        lng: this.locationInfo.longitude,\r\n      }).then(res => {\r\n        if (res.state) {\r\n          this.suitShops = res.info ? res.info.suitShops : []\r\n          this.recentShop =  res.info.suitShops ? res.info.suitShops[0] : {}\r\n        } else {\r\n          // this.back_to(res)\r\n        }\r\n      })\r\n    },\r\n    // 特卖获取推荐礼包和推荐权益\r\n    getColRecom(page = 1) {\r\n      this.$api.collect_recommend({\r\n        lat: this.locationInfo.latitude,\r\n        lng: this.locationInfo.longitude,\r\n        cardId: this.cardId,\r\n        detailId: this.detailId,\r\n        actSn: this.options.actSn,\r\n        cityId: this.locationCity.cityId,\r\n        page\r\n      }).then(res => {\r\n        if (res.state) {\r\n          if (page == 1) {\r\n            this.showRecom = true\r\n            this.collocation = res.info.collocation || {}\r\n            this.recomTotalPage = res.info.page.totalPage\r\n          }\r\n          this.actList = [...this.actList, ...res.info.actList]\r\n        }\r\n      })\r\n    },\r\n    // 特卖的推荐权益上拉更多\r\n    lowerdown() {\r\n      if (this.recomPage < this.recomTotalPage) {\r\n        this.recomPage = this.recomPage + 1\r\n        this.getColRecom(this.recomPage)\r\n      } else {\r\n        this.hasMore = false\r\n      }\r\n    },\r\n    // 大礼包获取其他礼包推荐\r\n    getRecomList() {\r\n      return this.$api.recommend_list({\r\n        actSn: this.actSn || this.options.actSn,\r\n        cityId: this.locationCity.cityId\r\n      }).then(res => {\r\n        if (res.state) {\r\n          this.recommendList = res.info.actList,\r\n          this.showRecom = true\r\n        }\r\n      })\r\n    },\r\n    // 获取限时限量结束弹窗的推荐活动\r\n    getEndActive() {\r\n      let type = this.options.type ? this.options.type : 1\r\n      this.$api.seckill_recommend({\r\n        lat: this.locationInfo.latitude,\r\n        lng: this.locationInfo.longitude,\r\n        cityId: this.locationCity.cityId,\r\n        timeId: this.options.timeId || 0,\r\n        marketActId: this.options.marketActId || 0,\r\n        type\r\n      }).then(res => {\r\n        if (res.state && res.info) {\r\n          this.recommendAct = res.info,\r\n          this.endShow = res.info.length ? true : false\r\n        }\r\n      })\r\n    },\r\n    // 页面错误 返回上一页 无上一页默认返回首页\r\n    back_to(res) {\r\n      this.$Toast(res.message)\r\n      console.log(this.$router)\r\n      setTimeout(() => {\r\n        if(this.$route.query.replace) {\r\n          this.$router.replace({\r\n            name: 'home'\r\n          })\r\n        } else {\r\n          this.$router.go(-1)\r\n        }\r\n      }, 500);\r\n    },\r\n  }\r\n}\r\nexport default entranceMixin "]}]}