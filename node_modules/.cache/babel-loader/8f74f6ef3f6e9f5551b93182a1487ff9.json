AA,UAVA,wBAUA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,SAAA,EAAA,YAAA,EAAA,KAAA,SAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,kBAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,MAAA,EAAA,KAAA,IAAA,CAAA,MAAA;AAAA,UAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QAAA;AAAA,UAAA,IAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAbA;AAcA;AACA,IAAA,SAfA,uBAeA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,WADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,GAAA,EAAA,KAAA,UAAA,CAAA,GADA;AAEA,UAAA,GAAA,EAAA,KAAA,UAAA,CAAA;AAFA;AAFA,OAAA;AAOA,KAvBA;AAwBA;AACA,IAAA,aAzBA,yBAyBA,CAzBA,EAyBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,YAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,MAAA,EAAA,KAAA,UAAA,CAAA;AAAA;AAAA,OAAA;AACA,KA3BA;AA6BA;AACA,IAAA,QA9BA,sBA8BA;AACA,WAAA,IAAA,GAAA,CAAA,KAAA,IAAA;AACA;AAhCA,GArBA;AAuDA,EAAA,OAvDA,qBAuDA;AACA,QAAA,OAAA,GAAA,KAAA,MAAA,CAAA,KAAA;AACA,IAAA,OAAA,CAAA,MAAA,GAAA,gBAAA;AACA,SAAA,OAAA,GAAA,OAAA;AACA;AA3DA,CAAA","sourcesContent":["<!--\r\n * @Description: 卡券详情\r\n * @Version: 1.0\r\n * @Autor: cherry\r\n * @Date: 2020-07-21 14:22:07\r\n * @LastEditors: yesong\r\n * @LastEditTime: 2020-08-12 19:04:48\r\n--> \r\n<template>\r\n  <div class=\"couponDetail\">\r\n    <div class=\"header-img\"></div>\r\n    <div class=\"card-detail\">\r\n      <div class=\"card-detail-code\">\r\n        <div class=\"shop-info flex-v\">\r\n          <img class=\"shop-log\" :src=\"info.shopPic\" />\r\n          <div class=\"ft30 c1A flex-1\">{{ info.shopName }}</div>\r\n          <div class=\"solid-left\"></div>\r\n          <div class=\"solid-right\"></div>\r\n        </div>\r\n        <div class=\"code-box\">\r\n          <card-code-detail :options=\"options\" @setInfo=\"setInfo\" />\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 卡券显示 -->\r\n      <div class=\"coupon-detail c1A\" v-if=\"info.cardType == 'CARD_STORAGE' || info.cardType == 'CARD_MULTI_COUPON'\">\r\n        <div class=\"ft32 til bold\">商品详情</div>\r\n        <template v-if=\"info.cardType == 'CARD_STORAGE'\">\r\n          <!-- 寄存卡 -->\r\n          <div class=\"flex-b\">\r\n            <div class=\"ft26\">{{ info.storageDetail.proName || '' }}({{ info.storageDetail.originalNum || 1 }}张)</div>\r\n            <div class=\"ft26\">已使用<span class=\"c9D\">{{ info.cardStorage.useNum || 0 }}</span>张，剩余{{ info.cardStorage.remainNum || 0 }}张</div>\r\n          </div>\r\n        </template>\r\n        <template v-if=\"info.cardType == 'CARD_MULTI_COUPON'\">\r\n          <!-- 套餐券 -->\r\n          <div v-for=\"(item, index) in info.comboDetail\" :key=\"index\">\r\n            <div class=\"ft26 c1A meal-line-til bold\">{{ item.categoryName || '品类' }}</div>\r\n            <div \r\n              class=\"meal-line-con flex-b ft26 c1A\"\r\n              v-for=\"(i, idx) in item.categoryList\" \r\n              :key='idx'\r\n            >\r\n              <div>{{ i.proName || '菜品' }}({{i.originalNum || 1}}份)</div>\r\n              <div>{{ i.cardWorth/100 }}元</div>\r\n            </div>\r\n          </div>\r\n        </template>\r\n      </div>\r\n\r\n      <!-- 适用店铺 -->\r\n      <div \r\n        class=\"apply-store\" \r\n        @click=\"toShopMap\"\r\n        v-if=\"suitShops.length > 0\"\r\n      >\r\n        <div class=\"ft32 c1A bold\">适用门店</div>\r\n        <div class=\"ft26 c1A shop-name\">{{ recentShop.shopName }}</div>\r\n        <div class=\"shop-detail flex-v\">\r\n          <div class=\"address flex-v\">\r\n            <span class=\"iconfont iconicon_address ft30 c9E\"></span>\r\n            <span class='ft24 c9E' style=\"margin-left: 7px;\"> {{ recentShop.distance | formatDistance }} | </span>\r\n          </div>\r\n          <div class='flex-1 t-elps dis-span ft24 c9E'>{{ recentShop.address }}</div>\r\n          <a @click.stop=\" \" :href=\"'tel:' + recentShop.tel\">\r\n            <div class=\"iconfont iconicon_tel ft34 c66 phone\"></div>\r\n          </a>\r\n        </div>\r\n        <div class=\"apply-more ft26 c1A flex-cc\" @click.stop=\"applyStore\" v-if=\"suitShops.length > 1\">\r\n          查看{{ suitShops.length }}家适用门店<span class=\"iconfont iconicon_more ft18 c9E\"></span>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 使用须知 -->\r\n      <div class=\"content-box c1A\">\r\n        <div class=\"ft32 bold cont-til\">使用须知</div>\r\n        <div>\r\n          <div class=\"ft28 bold til\">有效期</div>\r\n          <div class=\"ft26\">\r\n            <template v-if=\"info.validityType == 9\">{{ info.cardStartTime | formatTime('YYYY-MM-DD')}}-{{info.cardEndTime | formatTime('YYYY-MM-DD')}} </template>\r\n            <template v-else >{{ info.cardStartTime | formatTime('YYYY-MM-DD') }}-{{info.cardEndTime | formatTime('YYYY-MM-DD') }} </template>\r\n          </div>\r\n        </div>\r\n        <div> \r\n          <div class=\"ft28 bold til\">使用时间</div>\r\n          <div class=\"ft26\">{{ info.usePeriod ? info.limitTimeInterval : '不限时段，营业时间可用' }}</div>\r\n        </div>\r\n        <template v-if=\"info.description\">\r\n          <div class='ft28 bold til'>使用说明</div>\r\n          <div class='ft26 c1A notice'>\r\n            <span space id=\"des\">{{ info.description }}</span>\r\n          </div>\r\n        </template>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 返回首页浮窗 -->\r\n    <router-link\r\n      v-if=\"options.showBtn\"\r\n      class=\"return-icon\"\r\n      to=\"/index/home\"\r\n      >\r\n      <img src=\"http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.0.0/back_home.png\" />\r\n    </router-link>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n  import cardCodeDetail from '@/components/center/cardCodeDetail'\r\n  import { mapGetters } from 'vuex';\r\n  export default {\r\n    name: 'couponDetail',\r\n    mixins: [],\r\n    computed: {\r\n      ...mapGetters(['vipCardInfo'])\r\n    },\r\n    components:{\r\n      cardCodeDetail\r\n    },\r\n    data() {\r\n      return {\r\n        flag: true,\r\n        open: false,\r\n        flow: false,\r\n        actSn: '',\r\n        options: {},\r\n        suitShops: [],\r\n        info: {},\r\n        recentShop: {}\r\n      }\r\n    },\r\n    methods: {\r\n      // 设置页面信息\r\n      setInfo (info) {\r\n        let suitShops = info.suitShops || []\r\n        this.suitShops =suitShops\r\n        this.recentShop = suitShops[0] || {}\r\n        this.info = info\r\n      },\r\n\r\n      // 跳转适用店铺\r\n      applyStore () {\r\n        this.$save.set('session', 'applystore', this.suitShops)\r\n        this.$router.push({name: 'applyAddressShop', query: {cardId: this.info.cardId, detailId: this.info.detailId, type: 1}})\r\n      },\r\n      // 地图\r\n      toShopMap() {\r\n        this.$router.push({\r\n          name: 'commonMap',\r\n          query: {\r\n            lat: this.recentShop.lat,\r\n            lng: this.recentShop.lng\r\n          }\r\n        })\r\n      },\r\n      //跳转店铺详情\r\n      _toShopDetail(e) {\r\n        this.$router.push({name: 'shopDetail', query: {shopSn: this.recentShop.shopSn}})\r\n      },\r\n\r\n      // 套餐详情展开收起\r\n      lookMore () {\r\n        this.flag = !this.flag\r\n      },\r\n    },\r\n    created() {\r\n      let options = this.$route.query\r\n      options.apiUrl = 'myCouponDetail'\r\n      this.options = options\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n.header-img {\r\n  width: 100%;\r\n  height: 214px;\r\n  background: url('http://doyd.oss-cn-shenzhen.aliyuncs.com/vip_mini_apply/v1.4.1/img_bg_other_pange.png') no-repeat center;\r\n  background-size: cover;\r\n}\r\n.card-detail {\r\n  padding: 0 24px;\r\n  margin-top: -194px;\r\n}\r\n.card-detail-code {\r\n  border-radius: 12px;\r\n  overflow: hidden;\r\n}\r\n.code-box {\r\n  /* border-top-left-radius: 18px;\r\n  border-top-right-radius: 18px; */\r\n  background: #fff;\r\n  overflow: hidden;\r\n}\r\n.shop-info {\r\n  position: relative;\r\n  /* height: 100px; */\r\n  padding: 20px 26px;\r\n  background: #FAFAFA;\r\n  /* border-bottom-left-radius: 18px;\r\n  border-bottom-right-radius: 18px; */\r\n  box-sizing: border-box;\r\n}\r\n\r\n.shop-info .solid-right, .shop-info .solid-left{\r\n  position: absolute;\r\n  bottom: -9px;\r\n  left: -9px;\r\n  width: 18px;\r\n  height: 18px;\r\n  background: rgb(255, 207, 149, .8);\r\n  border-radius: 50%;\r\n}\r\n.shop-info .solid-right {\r\n  left: auto;\r\n  right: -9px;\r\n}\r\n\r\n.shop-info .shop-log {\r\n  width: 60px;\r\n  height: 60px;\r\n  border: 1px solid rgba(255,255,255,0.30);\r\n  border-radius: 6px;\r\n  margin-right: 18px;\r\n}\r\n.shop-info div {\r\n  line-height: 36px;\r\n}\r\n\r\n.apply-store {\r\n  padding: 30px 24px 0;\r\n  border-radius: 12px;\r\n  background-color: #fff;\r\n  margin-top: 18px;\r\n}\r\n.apply-store .shop-name {\r\n  margin: 30px 0 15px;\r\n}\r\n.apply-store .shop-detail {\r\n  padding-bottom: 33px;\r\n}\r\n.apply-store .dis-span {\r\n  width: 470px;\r\n  padding: 0 10px;\r\n  border-right: 1px solid #C2C3C5;\r\n  line-height: 1;\r\n}\r\n.apply-store .phone {\r\n  margin-left: 30px;\r\n}\r\n.apply-store .apply-more {\r\n  height: 72px;\r\n  border-top: 1px solid rgba(245, 243, 240, .8);\r\n}\r\n.apply-store .apply-more .iconfont {\r\n  margin-left: 9px;\r\n}\r\n\r\n.content-box {\r\n  margin-top: 18px;\r\n  padding: 30px 24px 40px;\r\n  border-radius: 12px;\r\n  background: #fff;\r\n}\r\n.content-box .cont-til {\r\n  padding-bottom: 8px;\r\n}\r\n.content-box .til {\r\n  margin: 24px 0 15px;\r\n}\r\n.content-box .notice {\r\n  line-height: 34px;\r\n}\r\n.coupon-detail {\r\n  margin-top: 18px;\r\n  padding: 30px 24px;\r\n  background: #fff;\r\n  border-radius: 12px;\r\n}\r\n.coupon-detail .meal-line-til {\r\n  margin-top: 18px;\r\n}\r\n.coupon-detail .meal-line-con {\r\n  padding-left: 24px;\r\n  margin-top: 18px;\r\n}\r\n.coupon-detail .til {\r\n  margin-bottom: 30px;\r\n}\r\ndiv,span {\r\n  line-height: 1;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n.header {\r\n  padding: 40px 30px 50px;\r\n}\r\n.bff {\r\n  background-color: #fff;\r\n}\r\n.bff-box {\r\n  padding-bottom: 50px;\r\n}\r\n.bff-box .address {\r\n  border-right: 2px solid #C2C3C5;\r\n  margin-right: 10px;\r\n  padding-right: 5px;\r\n}\r\n.bff-box .address .icon{\r\n  width: 22px;\r\n  height: 24px;\r\n  margin-right: 10px;\r\n}\r\n.bff-box .applyStore {\r\n  margin-top: 40px;\r\n}\r\n\r\n.header .shop img {\r\n  min-width: 80px;\r\n  max-width: 80px;\r\n  height: 80px;\r\n  margin-right: 20px;\r\n  border-radius: 14px;\r\n  border: 1px solid #E5E6E8;\r\n}\r\n.header .shop div {\r\n  color: #0D0D0D;\r\n  line-height: 38px;\r\n}\r\n.header .card {\r\n  margin-top: 30px;\r\n  /* padding-bottom: 40px; */\r\n  width: 100%;\r\n  background: #FFFFFF;\r\n  border: 1px solid #E4E4E4;\r\n  box-shadow: 0 2px 7px 0 rgba(226,226,226,0.50);\r\n  border-radius: 14px;\r\n}\r\n.header .card .title {\r\n  position: relative;\r\n  padding: 20px;\r\n  /* height: 124px; */\r\n  background: #F4F5F7;\r\n  border-radius: 14px 14px 0px 0px;\r\n}\r\n.header .card .title .bg {\r\n  position: absolute;\r\n  right: 0;\r\n  bottom: 0;\r\n  width: 112px;\r\n  height: 82px;\r\n}\r\n.header .card .title .card-name {\r\n  font-size: 40px;\r\n  color: #232532;\r\n  margin-bottom: 16px;\r\n}\r\n.header .card .title .rules div {\r\n  line-height: 32px;\r\n}\r\n.header .card .title .rules-time {\r\n  margin-top: 15px;\r\n  display: inline-block;\r\n  background: #E5E7EC;\r\n  border: 1px solid #E5E6E8;\r\n  border-radius: 3px;\r\n  padding: 4px 8px;\r\n}\r\n\r\n.header .card .title .num {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  height: 40px;\r\n  line-height: 40px;\r\n  padding: 0 15px;\r\n  background-image: linear-gradient(90deg, #474C61 0%, #222532 100%);\r\n  border-bottom-left-radius: 20px; \r\n  font-size: 22px;\r\n  color: #EEDCA2;\r\n}\r\n\r\n.header .card .slider {\r\n  margin-top: 20px;\r\n  padding-bottom: 40px;\r\n}\r\n.header .card .slider .content {\r\n  padding: 0 55px;\r\n  margin-top: 20px;\r\n  height: 270px;\r\n}\r\n.header .card .slider .content swiper {\r\n  width: 360px;\r\n  height:100%;\r\n  margin: 0 auto;\r\n}\r\n.header .card .slider .content .item-box{\r\n  position: relat