AAA,OAhCA,qBAgCA,CACA,CAjCA;AAkCA,EAAA,OAlCA,qBAkCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,eAAA,KAAA,CAAA,UAAA,IAAA,KAAA,CAAA,aAAA,EAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AApCA,CAAA","sourcesContent":["<!--\r\n * @Description: 客服\r\n * @Version: 1.0\r\n * @Autor: cherry\r\n * @Date: 2020-08-03 16:51:06\r\n * @LastEditors: cherry\r\n * @LastEditTime: 2020-08-08 10:23:17\r\n--> \r\n<template>\r\n  <div>\r\n    <template v-if=\"serviceList.length\">\r\n      <div @click=\"linkService\" >\r\n        <slot></slot>\r\n      </div>\r\n      <!-- 客服 -->\r\n      <van-action-sheet  v-model=\"serviceShow\"  cancel-text=\"取消\" @cancel=\"serviceShow = false\">\r\n        <div class=\"service-box\">\r\n          <div v-for=\"(item, index) in serviceList\" :key=\"index\">\r\n            <template  v-if=\"item.state\">\r\n              <a class=\"service-content flex-cc\"  :href=\"'tel:' + item.value1\" v-if=\"item.functionType === 'CUSTOMER_SERVICE_MANAGER_TEL_SERVICE'\">{{item.value1}}</a>\r\n              <div class=\"service-content flex-cc\" v-clipboard:copy=\"item.value1\" v-clipboard:success=\"serviceCopy\" v-else>{{item.value1}}</div>\r\n            </template>\r\n          </div>\r\n        </div>\r\n      </van-action-sheet>\r\n    </template>\r\n  </div>\r\n</template>\r\n<script>\r\n  import { mapGetters, mapActions} from \"vuex\";\r\n\r\n  export default {\r\n    name: 'service',\r\n    components: {},\r\n    props: {},\r\n    data() {\r\n      return {\r\n        serviceShow: false\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapGetters([\"moduleList\"]),\r\n      serviceList() {\r\n        if(!this.moduleList || !this.moduleList.length) return []\r\n        let list = this.moduleList.filter(item => {\r\n          return (item.functionType === 'CUSTOMER_SERVICE_MANAGER_TEL_SERVICE' && item.state) || (item.functionType  === 'CUSTOMER_SERVICE_MANAGER_WX_SERVICE' && item.state)\r\n        })\r\n        return list\r\n      }\r\n    },\r\n    methods: {\r\n      ...mapActions('index', [\r\n        'setModuleList'\r\n      ]),\r\n      // 点击客服\r\n      linkService() {\r\n        this.serviceShow = true\r\n      },\r\n      // 复制微信客服\r\n      serviceCopy(val) {\r\n        this.$Toast('已复制微信，请在微信中粘贴搜索')\r\n      },\r\n    },\r\n    mounted() {\r\n    },\r\n    async created() {\r\n      !this.moduleList && this.setModuleList()\r\n    }\r\n  }\r\n</script>\r\n<style scoped lang='scss'>\r\n  .service-box {\r\n    padding: 30px 0;\r\n    .service-content {\r\n      height: 80px;\r\n      margin-bottom: 10px;\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/common/common"}]}