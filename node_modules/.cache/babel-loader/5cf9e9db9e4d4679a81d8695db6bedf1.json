YAAJ,sBAAmCG,IAAnC,CAAhB;AACD,CAtLM,iEAyLOA,IAzLP,EAyLa;AAClB,SAAOJ,SAAS,WAAIC,YAAJ,+BAA4CG,IAA5C,CAAhB;AACD,CA3LM,+DA6LMA,IA7LN,EA6LY;AACjB,SAAOJ,SAAS,WAAIC,YAAJ,uBAAoCG,IAApC,CAAhB;AACD,CA/LM,yEAiMWA,IAjMX,EAiMiB;AACtB,SAAOJ,SAAS,WAAIC,YAAJ,4BAAyCG,IAAzC,CAAhB;AACD,CAnMM,2EAqMYA,IArMZ,EAqMkB;AACvB,SAAOJ,SAAS,WAAIC,YAAJ,6BAA0CG,IAA1C,CAAhB;AACD,CAvMM,yEAyMWA,IAzMX,EAyMiB;AACtB,SAAOJ,SAAS,WAAIC,YAAJ,gCAA6CG,IAA7C,CAAhB;AACD,CA3MM,mEA6MQA,IA7MR,EA6Mc;AACnB,SAAOJ,SAAS,WAAIC,YAAJ,yCAAsDG,IAAtD,CAAhB;AACD,CA/MM,QAAT;AAkNA,eAAeF,GAAf","sourcesContent":["/*\r\n * @Description: \r\n * @Version: 1.0\r\n * @Autor: cherry\r\n * @Date: 2020-07-22 11:27:16\r\n * @LastEditors: cherry\r\n * @LastEditTime: 2020-07-22 11:27:58\r\n */ \r\nimport wxRequest from '@utils/request'\r\nimport { VIP_APP_SHOP } from '@/config/index.js'\r\n\r\nconst api = {\r\n  // 首页banner\r\n  index_banner(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1.2.6/gift_discount/banner`, data)\r\n  },\r\n  // 公共banner\r\n  common_banner(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1.2.6/gift_discount/banner`, data)\r\n  },\r\n  // 优化更新banner\r\n  mall_banner(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_3_9/gift_discount/banner`, data)\r\n  },\r\n  // 会员超值礼包\r\n  vip_gift(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/gift_discount/vip_gift`, data)\r\n  },\r\n  // 会员日商家列表\r\n  vipday_shop(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/gift_discount/vipday/shop`, data)\r\n  },\r\n  // 二级商圈列表\r\n  businessCircleSec(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/business/circle`, data)\r\n  },\r\n  // 热门商圈列表\r\n  business_circle(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/gift_discount/hot/business/circle`, data)\r\n  },\r\n  // 热门商圈折扣商家列表\r\n  circle_discount(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/gift_discount/circle_discount/shop`, data)\r\n  },\r\n  // 买单折扣商家\r\n  buy_discount_shop(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/gift_discount/buy_discount/shop`, data)\r\n  },\r\n  // 获得城市子集区域\r\n  areaList(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/area/childList`, data)\r\n  },\r\n  // 品类列表\r\n  category(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/category`, data)\r\n  },\r\n  // 商家固定信息\r\n  shop_fixed_detail(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/fixed/detail`, data)\r\n  },\r\n  // 商家变动信息\r\n  shop_change_detail(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/change/details`, data)\r\n  },\r\n  // 商家买单折扣信息\r\n  act_buy_discount(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/activity/buy_discount`, data)\r\n  },\r\n  // 商家特卖优惠\r\n  act_special_sell(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/activity/special_sell`, data)\r\n  },\r\n  // 商家大礼包\r\n  act_giftbag(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/activity/giftbag`, data)\r\n  },\r\n  // 用户当前使用的折扣(最低的)\r\n  lowest_discount(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/use/discount`, data)\r\n  },\r\n  // 获取折扣信息详情\r\n  this_discount(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/discount`, data)\r\n  },\r\n  // 用户买单使用的卡券\r\n  buypay_card(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/use/card`, data)\r\n  },\r\n  // 用户买单可使用的红包\r\n  buypay_redpackage(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/use/redpackage`, data)\r\n  },\r\n  // 买单支付\r\n  buypay_pay(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/pay`, data)\r\n  },\r\n  // 买单支付结果查询\r\n  buypay_query(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/query`, data)\r\n  },\r\n  // 买单解除优惠券锁定\r\n  buypay_cancel(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/cancel`, data)\r\n  },\r\n  // 买单其他推荐\r\n  buypay_recommend(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/other_recommend`, data)\r\n  },\r\n  // 功能模块显示\r\n  module_show(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/module/show`, data)\r\n  },\r\n  // 会员买单订单列表\r\n  buypayOrderList(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/order/list`, data)\r\n  },\r\n\r\n  // 会员买单订单详情\r\n  buypayOrderDetail(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/buypay/order/detail`, data)\r\n  },\r\n  // 店铺分享图\r\n  shopSharePic(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2_3/shop/share/pic`, data)\r\n  },\r\n  // 首页弹窗广告\r\n  indexPopupad(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1.2.7/gift_discount/popupad`, data)\r\n  },\r\n  // 根据qrcode获取商家\r\n  getinfo_qrCode(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/getinfo/qrcode`, data)\r\n  },\r\n  // 商家折扣商圈信息(二级商圈v1_2)\r\n  business_circle_v2(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2/coupon/business/circle`, data)\r\n  },\r\n  // 商家折扣地区信息\r\n  business_area(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2/business/area`, data)\r\n  },\r\n  // 品类列表v1_2\r\n  business_category(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2/business/category`, data)\r\n  },\r\n  // 热门返现\r\n  cash_back(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2_3/cashback/homepage/shop`, data)\r\n  },\r\n  // 推荐商家\r\n  recommend_shop(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_3_9/recommend/shop`, data)\r\n  },\r\n  // 商家返现列表(v1_4_0)\r\n  activity_cashback(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_4_0/shop/activity/cashback`, data)\r\n  },\r\n  // 当前可用的最高返现（v1.2.3）\r\n  use_cashreta(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2_3/shop/buypay/use/cashreta`, data)\r\n  },\r\n  // 热门返现\r\n  hot_cashback(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_3_9/hot/cashback/shop`, data)\r\n  },\r\n  // 会员日\r\n  vipday_cashback(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2_3/vipday/cashback/shop`, data)\r\n  },\r\n  // 当前可用的最高返现（v1.2.3）\r\n  use_cashreta(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2_3/buypay/use/cashreta`, data)\r\n  },\r\n  // 会员钱包适用范围(v1.2.3)\r\n  wallet_scope(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_2_3/wallet/scope`, data)\r\n  },\r\n  // 首页浮窗v1.2.6\r\n  flot_window(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1.2.6/flot_window`, data)\r\n  },\r\n\r\n  // 红包可用商家\r\n  cash_red_able_shop(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1.2.7/cash_red_able_shop`, data)\r\n  },\r\n  // 热门城市\r\n  area_hot_list(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/area/hot`, data)\r\n  },\r\n  // 城市模块配置\r\n  city_config(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/city/config`, data)\r\n  },\r\n  // ...............小程序优化新增..........\r\n  // 筛选栏目列表\r\n  filtrate_list(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/filtrate/column/list`, data)\r\n  },\r\n  // 店铺收藏\r\n  shop_collect(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/collect`, data)\r\n  },\r\n  // 收藏列表\r\n  shop_collect_list(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/collect/list`, data)\r\n  },\r\n  // 查询店铺收藏\r\n  shop_query_collect(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1/shop/query/collect`, data)\r\n  },\r\n  // 店铺海报分享\r\n  shop_share_poster(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1_3_9/shop/share/poster`, data)\r\n  },\r\n  // 店铺其他推荐活动\r\n  shop_other_act(data) {\r\n    return wxRequest(`${VIP_APP_SHOP}/v1.4.0/shop/activity/special_sell`, data)\r\n  }\r\n}\r\n\r\nexport default api"]}]}