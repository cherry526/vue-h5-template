-9]+$/;\\n\\n  if (!reg.test(value)) {\\n    return false;\\n  } else {\\n    return true;\\n  }\\n}; // 验证输入的验证码（6位的正数字）\\n\\n\\nvar isAuthcode = function isAuthcode(num) {\\n  if (num) {\\n    var reg = /^\\\\d{6}$/;\\n    return !!reg.test(num);\\n  }\\n}; // 验证正整数\\n\\n\\nvar isNumber = function isNumber(num) {\\n  var reg = /^[0-9]\\\\d*$/;\\n  return !!reg.test(num);\\n}; // 验证用户密码（必须字母数字组合，6-16个字符）\\n\\n\\nvar isPassword = function isPassword(pass) {\\n  if (pass) {\\n    var reg = /^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{6,16}$/;\\n    return !!reg.test(pass);\\n  }\\n}; // 一位小数的正实数\\n\\n\\nvar isApoint = function isApoint(num) {\\n  if (num) {\\n    var reg = /^[0-9]+(.[0-9]{1})?$/;\\n    return !!reg.test(num);\\n  }\\n}; // 正数或一位小数\\n\\n\\nvar decimals = function decimals(num) {\\n  if (num) {\\n    var reg = /^[0-9]+([.][0-9]{1}){0,1}$/;\\n    return !!reg.test(num);\\n  }\\n}; //验证一个对象是否有值为空\\n\\n\\nvar emptyObj = function emptyObj(obj) {\\n  var res = true;\\n\\n  for (var key in obj) {\\n    if (!obj[key]) {\\n      res = false;\\n    }\\n  }\\n\\n  return res;\\n}; // 禁止为空、undefined和null\\n\\n\\nvar textNormal = function textNormal(str) {\\n  if (!str || str == 'undefined' || str == 'null') {\\n    return false;\\n  } else {\\n    return true;\\n  }\\n};\\n\\nvar NumSplit = function NumSplit() {\\n  var num = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 0;\\n  var split = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 1;\\n  var num2 = num / 100;\\n\\n  if (num % 10 == 0 && split == 1) {\\n    return num2.toFixed(1).split('.');\\n  } else {\\n    return num2.toFixed(2).split('.');\\n  }\\n}; //截取路由参数 参数格式在#之后\\n\\n\\nvar getDatas = function getDatas() {\\n  var url = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : window.location;\\n  var args = {};\\n  url = url.slice(url.indexOf('?') + 1);\\n\\n  if (url.length > 0) {\\n    var nameValueArr = url.split(\\\"&\\\"); // 多参数\\n\\n    nameVal